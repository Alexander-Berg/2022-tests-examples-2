.popup {
    position: absolute;
    box-sizing: border-box;

    user-select: none;

    opacity: 0;
    transition: opacity .1s ease-in, transform .1s ease-in;
    pointer-events: all;
}

    .popup_test {
        position: static;
        display: inline-block;
    }

    .popup {

    --transform-origin: top;
}

    .popup_direction_top {
            transform: translate(0, -5px);

            --tail-left: 50%;
            --tail-top: 100%;

            --tail-transform-x: 50%;
            --tail-transform-y: -100%;

            --tail-rotate: 180deg;
        }

    .popup_direction_left {
            transform: translate(-5px, 0);
            --transform-origin: 0 0;
            --tail-left: 100%;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 90deg;
        }

    .popup_direction_bottom {
            transform: translate(0, 5px);

            --tail-left: 50%;
            --tail-top: 1px;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 0deg;
        }

    .popup_direction_right {
            transform: translate(5px, 0);

            --tail-left: 0;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --transform-origin: 0 0;

            --tail-rotate: -90deg;
        }

    .popup_visible {
        opacity: 1;
        transform: none;
    }

    .popup__tail {
        position: absolute;
        display: flex;

        left: var(--tail-left);
        top: var(--tail-top);

        transform-origin: var(--transform-origin);
        transform: rotate(var(--tail-rotate)) translate(var(--tail-transform-x), var(--tail-transform-y));
    }

    .popup_overflow {
        overflow: auto;
    }
