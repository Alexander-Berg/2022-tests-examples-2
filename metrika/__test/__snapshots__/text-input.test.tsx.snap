    .text-input {

    position: relative;
    display: flex;
    box-sizing: border-box;
    width: 100%;
    vertical-align: top;
}

    .text-input__box {
        position: absolute;
        z-index: 1;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        height: auto;
        box-sizing: border-box;
        transition: background-color 0.15s linear, border-color 0.15s linear;
    }

    .text-input__control {
        position: relative;
        z-index: 2;
        box-sizing: border-box;
        width: 100%;
        font-size: var(--textinput-fontSize);
        height: var(--textinput-height);
        padding: 0 var(--textinput-control-side-space);
        margin: 0;
        vertical-align: top;
        color: var(--clr-el-gray-080);
        border: 0 solid transparent;
        outline: 0;
        background: none;
        -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    }

    .text-input__control::-webkit-search-decoration {
            appearance: none;
        }

    .text-input__control::-webkit-search-cancel-button {
            display: none;
        }

    .text-input__control::-ms-clear {
            display: none;
        }

    .text-input__control::placeholder {
            color: var(--clr-el-gray-050);
            opacity: 1; /* firefox */
        }

    /* TODO: improve autofill behavior */

    .text-input__control:-webkit-autofill {
            box-shadow: inset 0 0 0 50px #fff;
            -webkit-text-fill-color: var(--clr-el-gray-080);
        }

    .text-input:not(.text-input_prevent-outline):not(.text-input:active).text-input_view_default .text-input__control:focus-visible ~ .text-input__box {
        box-shadow: var(--shdw-el-focused);
    }

    .text-input__icon {
        z-index: 3;
        flex-shrink: 0;
        display: flex;
        align-items: center;
        justify-content: center;
        color: var(--clr-el-gray-050);
        transition: color 0.15s linear;
    }

    .text-input__icon_side_left {
            margin-left: var(--textinput-icon-side-space);
        }

    .text-input__icon_side_right {
            margin-right: var(--textinput-icon-side-space);
        }

    .text-input__icon_clear:hover {
            color: var(--clr-el-gray-060);
            cursor: pointer;
        }

    .text-input.text-input_icon-left .text-input__control {
        padding-left: var(--textinput-control-side-space-with-icon);
    }

    .text-input.text-input_icon-right .text-input__control {
        padding-right: var(--textinput-control-side-space-with-icon);
    }

    .text-input_disabled.text-input .text-input__box {
                border: none;
                background-color: var(--textinput-disabled-backgroundColor);
            }

    .text-input_disabled.text-input .text-input__control {
                color: var(--clr-el-gray-060);
                z-index: 3;
                cursor: default;
                user-select: none;
            }

    .text-input_disabled.text-input .text-input__control::placeholder {
                    color: var(--clr-el-gray-040);
                }

    .text-input_disabled.text-input .text-input__icon {
                color: var(--clr-el-gray-040);
            }

    .text-input_disabled.text-input .text-input__icon_clear {
                    cursor: default;
                }

    .text-input_view_default .text-input__box {
                border: var(--textinput-borderWidth) solid var(--clr-el-gray-030);
                border-radius: var(--textinput-view-default-borderRadius);
            }

    .text-input_view_default__control:-webkit-autofill {
                border-width: var(--textinput-borderWidth);
                border-radius: var(--textinput-view-default-borderRadius);
            }

    .text-input_view_default.text-input_error .text-input__box {
                border-color: var(--clr-el-red-060);
            }

    .text-input_view_borderless .text-input__box {
            border-bottom: var(--textinput-borderWidth) solid var(--clr-el-gray-030);
        }

    .text-input_view_borderless .text-input__box::after {
                position: absolute;
                right: 0;
                bottom: -1px;
                left: 0;
                content: '';
                pointer-events: none;
                border-bottom: 2px solid var(--clr-el-yellow-200);
                transition: transform 100ms cubic-bezier(0, 0, 0.2, 1) 0ms;
                transform: scaleX(0);
            }

    .text-input_view_clear .text-input__box {
            border: none;
        }

    .text-input_focused:not(.text-input_disabled):not(.text-input_error) .text-input__box {
            border-color: var(--clr-el-gray-040);
        }

    .text-input_focused:not(.text-input_disabled) .text-input__icon:not(.text-input__icon_clear) {
