.tag {
    --tag-dot-color: var(--clr-el-green-100);
    --tag-text-color: var(--clr-el-gray-080);
    --tag-caption-color: var(--clr-el-gray-060);
    --tag-closer-color: var(--clr-el-gray-050);
    --tag-icon-side-padding: 34px;

    display: inline-flex;
    flex-wrap: nowrap;
    cursor: pointer;
    position: relative;
    padding: var(--space-8) var(--space-16);
    background-color: var(--tag-bg-color);
    font-size: var(--font-size-16);
    line-height: var(--line-height-20);
    border-radius: var(--space-12);
    max-width: 100%;
    box-sizing: border-box;
    outline: none;
    transition: background-color 0.15s linear;
}

    .tag:focus-visible {
        box-shadow: var(--shdw-el-focused);
    }

    .tag_view_info:not(.tag_disabled) {
        --tag-bg-color: var(--clr-el-gray-020);
    }

    .tag_view_info:not(.tag_disabled):hover {
            --tag-bg-color: var(--clr-el-gray-030);
        }

    .tag_view_success:not(.tag_disabled) {
        --tag-bg-color: var(--clr-el-green-010);
        --tag-text-color: var(--clr-el-green-100);
        --tag-caption-color: var(--clr-el-green-060);
        --tag-closer-color: var(--clr-el-green-060);
    }

    .tag_view_success:not(.tag_disabled):hover {
            --tag-bg-color: var(--clr-el-green-020);
        }

    .tag_view_success:not(.tag_disabled) .tag__closer:hover {
            --tag-closer-color: var(--clr-el-green-100);
        }

    .tag_view_warning:not(.tag_disabled) {
        --tag-bg-color: var(--clr-el-yellow-010);
    }

    .tag_view_warning:not(.tag_disabled):hover {
            --tag-bg-color: var(--clr-el-yellow-020);
        }

    .tag_view_error:not(.tag_disabled) {
        --tag-bg-color: var(--clr-el-red-010);
        --tag-dot-color: var(--clr-el-red-100);
        --tag-text-color: var(--clr-el-red-100);
        --tag-caption-color: var(--clr-el-red-060);
        --tag-closer-color: var(--clr-el-red-040);
    }

    .tag_view_error:not(.tag_disabled):hover {
            --tag-bg-color: var(--clr-el-red-020);
        }

    .tag_view_error:not(.tag_disabled) .tag__closer:hover {
            --tag-closer-color: var(--clr-el-red-100);
        }

    .tag_disabled {
        --tag-bg-color: var(--clr-el-gray-020);
        --tag-text-color: var(--clr-el-gray-050);
        --tag-dot-color: var(--clr-el-gray-050);
        --tag-caption-color: var(--clr-el-gray-050);

        pointer-events: none;
        cursor: default;
    }

    .tag_multiline {
        flex-direction: column;
        width: 100%;
    }

    .tag_with-dot {
        padding-left: var(--tag-icon-side-padding);
    }

    .tag_with-closer {
        padding-right: var(--tag-icon-side-padding);
    }

    .tag_with-dot:before {
        content: '';
        display: block;
        position: absolute;
        top: 15px;
        left: 15px;
        width: var(--space-6);
        height: var(--space-6);
        border-radius: 50%;
        background-color: var(--tag-dot-color);
    }

    .tag__closer {
        position: absolute;
        top: var(--space-10);
        right: var(--space-10);
        height: var(--space-16);
        color: var(--tag-closer-color);
        outline: none;
        transition: color 0.15s linear;
    }

    .tag__closer:focus-visible {
            box-shadow: var(--shdw-el-focused);
        }

    .tag__closer:hover {
            --tag-closer-color: var(--clr-el-gray-060);
        }

    .tag__text {
        color: var(--tag-text-color);
    }

    .tag__caption {
        color: var(--tag-caption-color);
        margin-left: var(--space-8);
    }

    .tag__text,
    .tag__caption {
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }

    .tag_multiline .tag__text {
        line-height: var(--line-height-24);
    }

    .tag_multiline .tag__caption {
        font-size: var(--font-size-12);
        line-height: var(--line-height-16);
        margin: 0;
    }
.tag-stories {
    width: 200px;
}

    .tag-stories__tag {
        margin: 8px;
    }

