}
.button {
    border: none;
    cursor: pointer;
    background-color: transparent;
    padding: 0;
    outline: none;
}
    .button:focus-visible .button__shape {
        box-shadow: var(--shdw-el-focused);
    }
    .button__shape {
        display: flex;
        align-items: center;
        justify-content: center;
        transition: transform 0.1s ease-out, color 0.15s linear;
        color: var(--clr-el-gray-080);
        border-radius: var(--button-border-radius);
    }
    .button_rounded .button__shape {
        border-radius: calc(
            (
                    var(--button-mode-text-vertical-padding) * 2 + var(--button-line-height) +
                        var(--button-text-padding-bottom)
                ) / 2
        );
    }
    .button:active .button__shape {
        transition: none;
        transform: scale(0.96);
    }
    .button:disabled .button__shape,
    .button:disabled:hover .button__shape {
        transform: scale(1);
        background-color: var(--clr-el-gray-020);
        color: var(--clr-el-gray-050);
        cursor: default;
    }
    .button_mode_icon .button__shape {
            padding: var(--button-mode-icon-padding);
        }
    .button_mode_text .button__shape {
            padding: var(--button-mode-text-padding);
        }
    .button_with-left-icon .button__shape {
        padding-left: var(--button-mode-text-with-icon-horizontal-padding);
    }
    .button_with-right-icon .button__shape {
        padding-right: var(--button-mode-text-with-icon-horizontal-padding);
    }
    .button_size_s32 {
            --button-border-radius: 10px;
            --button-font-size: 14px;
            --button-line-height: 20px;

            --button-mode-text-vertical-padding: 5px;
            --button-mode-text-padding: var(--button-mode-text-vertical-padding) var(--space-12);
            --button-mode-text-with-icon-horizontal-padding: var(--space-8);
            --button-text-padding-bottom: var(--space-2);

            --button-mode-icon-padding: var(--space-8);
        }
    .button_size_s36 {
            --button-border-radius: 12px;
            --button-font-size: 14px;
            --button-line-height: 20px;

            --button-mode-text-padding: var(--space-8) var(--space-16);
            --button-mode-text-with-icon-horizontal-padding: var(--space-12);
            --button-text-padding-bottom: 0;

            --button-mode-icon-padding: 10px;
        }
    .button_size_s44 {
            --button-border-radius: 14px;
            --button-font-size: 16px;
