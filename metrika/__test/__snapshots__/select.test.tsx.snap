.svg-direction-icon {
    transition-duration: 0.1s;
    transition-property: transform;
}

    .svg-direction-icon_direction_left {
            transform: rotate(90deg);
        }

    .svg-direction-icon_direction_right {
            transform: rotate(-90deg);
        }

    .svg-direction-icon_direction_up {
            transform: rotate(180deg);
        }

    .svg-direction-icon_direction_down {
            transform: rotate(0deg);
        }
.popup {
    position: absolute;
    box-sizing: border-box;

    user-select: none;

    opacity: 0;
    transition: opacity .1s ease-in, transform .1s ease-in;
    pointer-events: all;
}

    .popup_test {
        position: static;
        display: inline-block;
    }

    .popup {

    --transform-origin: top;
}

    .popup_direction_top {
            transform: translate(0, -5px);

            --tail-left: 50%;
            --tail-top: 100%;

            --tail-transform-x: 50%;
            --tail-transform-y: -100%;

            --tail-rotate: 180deg;
        }

    .popup_direction_left {
            transform: translate(-5px, 0);
            --transform-origin: 0 0;
            --tail-left: 100%;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 90deg;
        }

    .popup_direction_bottom {
            transform: translate(0, 5px);

            --tail-left: 50%;
            --tail-top: 1px;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 0deg;
        }

    .popup_direction_right {
            transform: translate(5px, 0);

            --tail-left: 0;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --transform-origin: 0 0;

            --tail-rotate: -90deg;
        }

    .popup_visible {
        opacity: 1;
        transform: none;
    }

    .popup__tail {
        position: absolute;
        display: flex;

        left: var(--tail-left);
        top: var(--tail-top);

        transform-origin: var(--transform-origin);
        transform: rotate(var(--tail-rotate)) translate(var(--tail-transform-x), var(--tail-transform-y));
    }

    .popup_overflow {
        overflow: auto;
    }
.dropdown__popup {
        background-color: var(--clr-bg-gray-1);
        border-radius: 12px;
        box-shadow: var(--shdw-popover);
        overflow: hidden;
    }
    .dropdown__anchor,
    .dropdown__anchor-arrow {
        display: inline-block;
        user-select: none;
    }
    .dropdown__anchor:focus {
        outline: none;
    }
    .dropdown__anchor:focus-visible {
        box-shadow: var(--shdw-el-focused);
    }
    .dropdown__anchor-arrow {
        height: 16px;
    }
.options-list__item-container {
        padding: var(--space-8) var(--space-12) var(--space-8) var(--space-24);
        display: flex;
        justify-content: space-between;
        cursor: pointer;
        user-select: none;
    }

        .options-list__item-container:hover:not(.options-list__item-container_active):not(.options-list__item-container_disabled) {
            background-color: var(--clr-bg-gray-2);
        }

        .options-list__item-container_active {
            background-color: var(--clr-bg-gray-3);
        }

        .options-list__item-container_centered-content {
            align-items: center;
        }

        .options-list__item-container_disabled {
            cursor: default;
        }
    .options-list_width_auto {
        max-width: max-content;
    }
    .options-list__icon-container {
        margin-left: var(--space-12);
        width: 16px;
        height: 16px;
    }
    .options-list__icon {
        color: var(--clr-el-gray-080);
    }
    .options-list__item {
        display: flex;
        flex-direction: column;
        flex: 1;
        word-break: break-word;
        overflow-wrap: anywhere;
    }
    .options-list__item-header {
        display: flex;
        justify-content: space-between;
    }
    .options-list__item-title {
        color: var(--clr-el-gray-080);
        font-size: 14px;
        line-height: 20px;
    }
    .options-list__item-container_disabled .options-list__item-title {
        color: var(--clr-el-gray-050);
    }
    .options-list__item-subtitle {
        color: var(--clr-el-gray-060);
        font-size: 14px;
        line-height: 20px;
        margin-left: var(--space-12);
    }
    .options-list__item-container_disabled .options-list__item-subtitle {
        color: var(--clr-el-gray-050);
    }
    .options-list__item-description {
        color: var(--clr-el-gray-060);
        font-size: 14px;
        line-height: 20px;
    }
    .options-list__item-container_disabled .options-list__item-description {
        color: var(--clr-el-gray-050);
    }
.select {
    padding: var(--space-10) 0;
    min-width: max-content !important;
}

