.popup {
    position: absolute;
    box-sizing: border-box;

    user-select: none;

    opacity: 0;
    transition: opacity .1s ease-in, transform .1s ease-in;
    pointer-events: all;
}

    .popup_test {
        position: static;
        display: inline-block;
    }

    .popup {

    --transform-origin: top;
}

    .popup_direction_top {
            transform: translate(0, -5px);

            --tail-left: 50%;
            --tail-top: 100%;

            --tail-transform-x: 50%;
            --tail-transform-y: -100%;

            --tail-rotate: 180deg;
        }

    .popup_direction_left {
            transform: translate(-5px, 0);
            --transform-origin: 0 0;
            --tail-left: 100%;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 90deg;
        }

    .popup_direction_bottom {
            transform: translate(0, 5px);

            --tail-left: 50%;
            --tail-top: 1px;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --tail-rotate: 0deg;
        }

    .popup_direction_right {
            transform: translate(5px, 0);

            --tail-left: 0;
            --tail-top: 50%;

            --tail-transform-x: -50%;
            --tail-transform-y: -100%;

            --transform-origin: 0 0;

            --tail-rotate: -90deg;
        }

    .popup_visible {
        opacity: 1;
        transform: none;
    }

    .popup__tail {
        position: absolute;
        display: flex;

        left: var(--tail-left);
        top: var(--tail-top);

        transform-origin: var(--transform-origin);
        transform: rotate(var(--tail-rotate)) translate(var(--tail-transform-x), var(--tail-transform-y));
    }

    .popup_overflow {
        overflow: auto;
    }
.main-menu__nested-popup {
        background-color: var(--clr-bg-gray-1);
        border-radius: 6px;
        box-shadow: var(--shdw-popover);
        overflow-y: auto;
        overflow-x: hidden;
        min-width: 300px;
        padding: var(--space-16) 0;
        box-sizing: border-box;
    }

        .main-menu__nested-popup_full-height {
            height: 100%;
        }
    .main-menu__nested-popup-content {
        max-width: 344px;
        box-sizing: border-box;
    }
    .main-menu__nested-group-title {
        padding: var(--space-8) 10px var(--space-8) var(--space-20);
        margin: 0;
        font-size: 14px;
        line-height: 20px;
        color: var(--clr-el-gray-060);
    }
    .main-menu__nested-group-breaker {
        margin: 10px 0;
        width: 100%;
        height: 1px;
        background-color: var(--clr-el-gray-030);
    }
.main-menu__root-item {
        padding: var(--main-menu-item-vertical-padding) var(--space-12);
        color: var(--clr-el-gray-060);
        cursor: pointer;
        display: flex;
        border-radius: 12px;
        align-items: center;
        box-sizing: border-box;
    }

        .main-menu__root-item_selected {
            background-color: var(--clr-bg-gray-2);
            color: var(--clr-el-gray-080);
        }

        .main-menu__root-item:hover {
            background-color: var(--clr-el-gray-020);
            color: var(--clr-el-gray-060);
        }
    .main-menu_compact .main-menu__root-item {
        height: calc(var(--main-menu-item-line-height) + var(--main-menu-item-vertical-padding) * 2);
    }
    .main-menu__root-item-title-container {
        display: flex;
        flex: 1;
        align-items: center;
        overflow: hidden;
        margin-left: var(--space-12);
    }
    .main-menu_compact .main-menu__root-item-title-container {
        overflow: unset;
    }
    .main-menu__root-item-icon {
        display: flex;
        margin: var(--space-2) 0;
    }
    .main-menu__root-item-title {
        font-size: 14px;
        line-height: 20px;
        font-weight: 500;
    }
    .main-menu__root-item-tag {
        margin-left: var(--space-12);
    }
    .main-menu__root-item-title,
    .main-menu__root-item-tag {
        transition: opacity 0.2s 0.1s;
    }
    .main-menu_compact .main-menu__root-item-title,
    .main-menu_compact .main-menu__root-item-tag {
        opacity: 0;
    }
.tooltip {
    display: flex;
    color: var(--tooltip-main-color);
    background-color: var(--tooltip-background);
    box-shadow: var(--shdw-popover);
}

    .tooltip_size_s {
            --title-font-weight: 400;
            font-size: var(--font-size-14);
            line-height: var(--line-height-20);
            border-radius: 10px;
            padding: 6px 12px;
        }

    .tooltip_size_m {
            --title-font-weight: 500;
            font-size: var(--font-size-16);
            border-radius: var(--line-height-24);
            border-radius: 14px;
            padding: 14px 20px;
        }

    .tooltip__icon {
        height: 24px;
        margin: 0 10px 0 -6px;
    }

    .tooltip__text {
        max-width: 320px;
    }

    .tooltip__title {
        font-weight: var(--title-font-weight);
    }

    .tooltip__description {
        color: var(--tooltip-additional-color);
    }

    .tooltip__close {
        margin: 3px -6px 0 10px;
        height: 16px;
    }

    .tooltip__close-icon {
            cursor: pointer;
            color: var(--tooltip-additional-color);
        }

    .tooltip__close-icon:hover {
                color: var(--tooltip-main-color);
            }

    .tooltip__tail {
        color: var(--tooltip-background);
    }

    .tooltip_view_dark {
            --tooltip-background: var(--clr-str-black-900);
            --tooltip-main-color: var(--clr-str-white-080);
            --tooltip-additional-color: var(--clr-str-white-060);
        }

    .tooltip_view_white {
            --tooltip-background: var(--clr-bg-gray-1);
            --tooltip-main-color: var(--clr-el-gray-080);
            --tooltip-additional-color: var(--clr-el-gray-060);
        }

    .tooltip_view_gray {
            --tooltip-background: var(--clr-bg-gray-3);
            --tooltip-main-color: var(--clr-el-gray-080);
            --tooltip-additional-color: var(--clr-el-gray-060);
        }

    .tooltip_view_red {
            --tooltip-background: var(--clr-el-red-100);
            --tooltip-main-color: var(--clr-str-white-100);
            --tooltip-additional-color: var(--clr-str-pure-white-060);
        }
.skeleton {
    background-color: var(--clr-el-gray-030);
}

.skeleton_fade-in {
    animation: fade-in 1s 1;
}

@keyframes fade-in {
    0% {
        opacity: 0;
    }

    30% {
        opacity: 0.1;
    }

    50% {
        opacity: 0.4;
    }

    100% {
        opacity: 1;
    }
}
.main-menu-stories {
    background-color: var(--clr-bg-app);
    display: flex;
}

    .main-menu-stories__right-content {
        background-color: var(--clr-bg-gray-1);
        display: flex;
        flex: 1;
    }

    .main-menu-stories__menu-content {
        overflow-x: unset;
        overflow-y: unset;
    }

