.user-account {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    cursor: pointer;
    overflow: hidden;
}

    .user-account_size_s32 {
            --avatar-width: 32px;
            --avatar-height: 32px;

            --account-info-margin-left: var(--space-6);

            --login-font-size: var(--font-size-14);
            --login-line-height: var(--line-height-20);

            --name-font-size: var(--font-size-14);
            --name-line-height: var(--line-height-18);
        }

    .user-account_size_s32.user-account_has-representative {
                --login-font-size: var(--font-size-12);
                --login-line-height: var(--line-height-14);
            }

    .user-account_size_s48 {
            --avatar-width: 48px;
            --avatar-height: 48px;

            --account-info-margin-left: var(--space-12);

            --login-font-size: var(--font-size-16);
            --login-line-height: var(--line-height-24);

            --name-font-size: var(--font-size-14);
            --name-line-height: var(--line-height-20);
        }

    .user-account_view_default {
            --avatar-opacity: 1;

            --login-first-letter-color: var(--clr-el-red-100);
            --login-color: var(--clr-el-gray-080);

            --name-color: var(--clr-el-gray-060);
        }

    .user-account_view_pale {
            --avatar-opacity: 0.4;

            --login-first-letter-color: var(--clr-el-gray-060);
            --login-color: var(--clr-el-gray-060);

            --name-color: var(--clr-el-gray-060);
        }

    .user-account_view_accent {
            --avatar-opacity: 1;

            --login-first-letter-color: var(--clr-el-red-100);
            --login-color: var(--clr-el-red-100);

            --name-color: var(--clr-el-red-060);
        }

    .user-account__avatars {
        display: flex;
        align-items: center;
        max-width: 100%;
    }

    .user-account_has-representative .user-account__avatar-container:not(.user-account__avatar-container_type_representative) {
        --avatar-opacity: 0.4;
    }

    .user-account__avatar-container {
        display: flex;
        flex-shrink: 0;
        overflow: hidden;
        border-radius: 50%;
        background: var(--clr-bg-gray-3);
        position: relative;
        width: var(--avatar-width);
        height: var(--avatar-height);
        opacity: var(--avatar-opacity);
    }

    .user-account__avatar-container_type_representative {
            border: 2px solid var(--clr-bg-gray-3);
            margin-left: calc(var(--space-16) * -1);
            z-index: 1;
            --avatar-opacity: 1;
        }

    .user-account__avatar {
        border-radius: 50%;
    }

    .user-account__account-info {
        margin-left: var(--account-info-margin-left);
        overflow: hidden;
        display: flex;
        flex-direction: column;
    }

    .user-account_has-representative .user-account__login:not(.user-account__login_type_representative) {
        color: var(--clr-el-gray-060);
    }

    .user-account_has-representative .user-account__login:not(.user-account__login_type_representative)::first-letter {
            color: var(--clr-el-gray-060);
        }

    .user-account__login {
        font-weight: 500;
        color: var(--login-color);
        font-size: var(--login-font-size);
        line-height: var(--login-line-height);
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
    }

    .user-account__login::first-letter {
            color: var(--login-first-letter-color);
        }

    .user-account__login_type_representative {
            --login-first-letter-color: var(--clr-el-red-100);
            --login-color: var(--clr-el-red-100);
            font-size: var(--font-size-14);
            line-height: var(--line-height-18);
        }

    .user-account__name {
        color: var(--name-color);
        font-size: var(--name-font-size);
        line-height: var(--name-line-height);
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
    }

    .user-account_hoverable:hover .user-account__login,
    .user-account_hoverable:hover .user-account__name {
        opacity: .6;
    }

