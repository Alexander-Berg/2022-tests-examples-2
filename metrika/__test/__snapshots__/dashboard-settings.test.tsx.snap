    --date-input-space-right: var(--space-20);
    --date-inputs-delimiter-width: 12px;
    --date-period-preset-margin: 6px;

    display: inline-block;
    border-radius: 12px;
    overflow: hidden;
    background-color: var(--clr-bg-gray-1);
    width: 586px;
}

    .date-range-picker__date-picker {
        display: flex;
        border-bottom: 1px solid var(--clr-el-gray-030);
    }

    .date-range-picker__presets {
        padding: 20px 24px calc(20px - var(--date-period-preset-margin)) 24px;
        display: flex;
        flex-wrap: wrap;
        border-bottom: 1px solid var(--clr-el-gray-030);
    }

    .date-range-picker__preset-item {
        color: var(--clr-el-gray-080);
        font-size: 14px;
        background-color: var(--clr-el-gray-020);
        border-radius: 8px;
        line-height: 20px;
        padding: 6px 12px;
        margin: 0 var(--date-period-preset-margin) var(--date-period-preset-margin) 0;
        cursor: pointer;
    }

    .date-range-picker__preset-item_selected {
            background-color: var(--clr-el-gray-040);
        }

    .date-range-picker__preset-item:hover:not(.period-select__preset-item_selected) {
             background-color: var(--clr-el-gray-030);
        }

    .date-range-picker__footer {
        display: flex;
        justify-content: space-between;
        padding: 20px 24px;
    }

    .date-range-picker__period-input {
        display: flex;
        align-items: center;
    }

    .date-range-picker__date-input {
        width: 120px;
        flex-shrink: 0;
    }

    .date-range-picker__date-input:first-child {
            position: relative;
            padding-right: var(--date-input-space-right);
        }

    .date-range-picker__date-input:first-child:after {
                content: '';
                display: block;
                position: absolute;
                top: 50%;
                right: calc((var(--date-input-space-right) - var(--date-inputs-delimiter-width)) / 2);
                width: var(--date-inputs-delimiter-width);
                border-bottom: 1px solid var(--clr-el-gray-050);
            }

    .date-range-picker__selected-period-length {
        margin: 0 12px;
        color: var(--clr-el-gray-060);
    }

    .date-range-picker__selected-period-length_error {
            color: var(--clr-el-red-060);
        }
.period-select__control {
         background-color: var(--clr-el-gray-000);
         box-shadow: var(--shdw-popover);
         border-radius: 12px;
    }
.metrics-aux-settings-menu__toggle-button {
        position: relative;
    }
    .metrics-aux-settings-menu__warning-icon {
        color: var(--clr-el-blue-100);
        position: absolute;
        top: -6px;
        right: -10px;
        opacity: 1;
        transition: opacity 0.3s linear;
    }
    .metrics-aux-settings-menu__warning-icon_hidden {
            opacity: 0;
        }
    .metrics-aux-settings-menu__accuracy-title {
        position: relative;
    }
    .metrics-aux-settings-menu__accuracy-title_changed:after {
            content: '';
            position: absolute;
            top: 7px;
            right: -14px;
            width: 8px;
            height: 8px;
            border-radius: 50%;
            background-color: var(--clr-el-blue-100);
        }
.skeleton {
    background-color: var(--clr-el-gray-030);
}

.skeleton_fade-in {
    animation: fade-in 1s 1;
}

@keyframes fade-in {
    0% {
        opacity: 0;
    }

    30% {
        opacity: 0.1;
    }

    50% {
        opacity: 0.4;
    }

    100% {
        opacity: 1;
    }
}
.multi-story {
    font-size: 16px;
}

    .multi-story__group {
        margin-bottom: 24px;
    }

    .multi-story__group-title {
        font-weight: 700;
        margin-bottom: 12px;
        white-space: pre;
    }

    .multi-story__group-items {
        display: flex;
    }

    .multi-story__item {
        margin-right: 32px;
    }

    .multi-story__item-title {
        margin-bottom: 8px;
    }

