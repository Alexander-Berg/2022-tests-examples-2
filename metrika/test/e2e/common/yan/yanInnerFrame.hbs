{{#*inline "content"}}
    Yan inner frame
    <script type="text/javascript">
        new Ya.Metrika2({
            id: 2
        });
        window._socket.on('request', function (r) {
            // console.log('inner ' + JSON.stringify(r) + ' ' + _socket.id);
        });
        window.addEventListener('message', function (event) {
            if (!event.data) {
                return;
            }
            var info = false;
            try {
                info = JSON.parse(event.data);
            } catch (e) { }
            // console.log(JSON.stringify(info));
            window.Ya._metrika.dataLayer.push(info);
        });
    </script>
