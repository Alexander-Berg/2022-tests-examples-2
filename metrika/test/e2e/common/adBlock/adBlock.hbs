{{#> base}}
    {{#*inline "title"}}
        AdBlock page title
    {{/inline}}
    {{#*inline "content"}}
        Adblock page
        <script type="text/javascript">
            function createHandleRequestFunction(done) {
                var requestsToCheck = [];

                return function handleOnRequest (request) {
                    requestsToCheck.push(request);

                    if (requestsToCheck.length === 2) {
                        done(requestsToCheck);
                    }
                };
            }
        </script>
    {{/inline}}
{{/base}}
