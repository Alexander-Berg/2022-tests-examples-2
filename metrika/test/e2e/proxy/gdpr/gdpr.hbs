{{#> base}}
{{#*inline "title"}}
    yandex page title
{{/inline}}
{{#*inline "defHeadCounterScripts"}}
    <script type="text/javascript" src="{{ buildPath }}/e2e/tag.js"></script>
{{/inline}}
{{#*inline "head"}}
<script type="text/javascript">
    if (!document.cookie.match(/forceIO/)) {
        setForceSessionCookie();
    }
</script>
{{/inline}}

{{#*inline 'defBodyScripts' }}
<script type="text/javascript">
    window._socket = io('{{ protocol }}://{{ host }}{{ portInfo }}');
    window.dispatchEvent(new Event('socket'));
</script>
{{/inline}}

{{#*inline "content"}}
    yandex page
{{/inline}}
{{/base}}