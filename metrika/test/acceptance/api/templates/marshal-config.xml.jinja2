<yandex>
    <master>
        <port>{{ port }}</port>
        <number_of_workers type='int'>1</number_of_workers>
        <logger_settings>
            <stdout>True</stdout>
            <log_format><![CDATA[%(asctime)s %(threadName)-12.12s %(processName)-12.12s %(name)48.48s %(levelname)-.1s %(message)s]]></log_format>
        </logger_settings>
        <loggers type='list'>
            <item type='str'>metrika</item>
            <item type='str'>marshal</item>
            <item type='str'>mtutils</item>
            <item type='str'>requests</item>
            <item type='str'>urllib3</item>
        </loggers>
    </master>
    <database>
        <name type='str' from_env_or_default='POSTGRES_RECIPE_DBNAME'>cms_test</name>
        <user type='str' from_env_or_default='POSTGRES_RECIPE_USER'>cms</user>
        <password type='str' from_env_or_default='POSTGRES_RECIPE_PASSWORD'>cms</password>
        <host type='str' from_env_or_default='POSTGRES_RECIPE_HOST'>127.0.0.1</host>
        <port type='int' from_env_or_default='POSTGRES_RECIPE_PORT'>5432</port>
        <conn_max_age type='int'>600</conn_max_age>
        <connect_timeout type='int'>2</connect_timeout>
    </database>
    <queue>{{ marshal_queue }}</queue>
    <lock>{{ marshal_lock }}</lock>
    <agent_port>{{ agent_port}}</agent_port>
    <frontend>{{ frontend }}</frontend>
    <walle>{{ walle }}</walle>
    <iteration_idle_time>1</iteration_idle_time>
    <agent_operation_poll_period>1</agent_operation_poll_period>
    <poll_period>0</poll_period>
    <postpone_loading_initiate>1</postpone_loading_initiate>
    <postpone_loading_initiate>1</postpone_loading_initiate>
    <postpone_loading_poll>1</postpone_loading_poll>
</yandex>
