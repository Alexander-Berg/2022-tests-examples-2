{
  "PASSPORT_BB_YANDEX_TEAM_URL": {
    "$mockserver": "/blackbox-team"
  },
  "SUPPORT_AUTHPROXY_CORS": {
    "allowed-headers": [],
    "allowed-origins": [
      "localhost"
    ],
    "cache-max-age-seconds": 60,
    "disabled-hosts": []
  },
  "SUPPORT_AUTHPROXY_ROUTE_RULES": {
    "rules": [
      {
        "input": {
          "http-path-prefix": "/support/v1/auth"
        },
        "output": {
          "tvm-service": "mock",
          "upstream": {
            "$mockserver": ""
          }
        },
        "proxy": {
          "auth": "session",
          "proxy-401": false,
          "server-hosts": [
            "*"
          ]
        }
      },
      {
        "input": {
          "http-path-prefix": "/support/v1/noauth"
        },
        "output": {
          "tvm-service": "mock",
          "upstream": {
            "$mockserver": ""
          }
        },
        "proxy": {
          "auth": "session",
          "proxy-401": true,
          "server-hosts": [
            "*"
          ]
        }
      },
      {
        "input": {
          "http-path-prefix": "/csrf_token"
        },
        "output": {
          "tvm-service": "mock",
          "upstream": {
            "$mockserver": ""
          }
        },
        "proxy": {
          "auth": "session-csrf-token-generator",
          "proxy-401": false,
          "server-hosts": [
            "*"
          ]
        }
      },
      {
        "input": {
          "http-path-prefix": "/csrf_token_noauth"
        },
        "output": {
          "tvm-service": "mock",
          "upstream": {
            "$mockserver": ""
          }
        },
        "proxy": {
          "auth": "session-csrf-token-generator",
          "proxy-401": true,
          "server-hosts": [
            "*"
          ]
        }
      }
    ]
  }
}
