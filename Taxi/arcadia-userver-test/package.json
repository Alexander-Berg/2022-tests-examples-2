      "source": {
        "path": "{package_root}/deploy/build/services/arcadia-userver-test/units/arcadia-userver-test/configs/config_vars.production.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/etc/yandex/taxi/arcadia-userver-test/config_vars.stress.yaml"
