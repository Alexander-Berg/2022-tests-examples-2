{
  "template" : "taxi-timings-*",
  "settings" : {
    "index.refresh_interval" : "10s",
    "index.number_of_shards" : 3,
    "index.routing.allocation.total_shards_per_node": 3
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : false},
      "numeric_detection": false,
      "date_detection": false,
      "dynamic_templates" : [ {
        "message_field" : {
          "match" : "message",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      }, {
        "boolean_fields" : {
          "match" : "*",
          "match_mapping_type" : "boolean",
          "mapping" : {
            "type" : "boolean", "doc_values" : true, "index" : "not_analyzed",
            "norms": { "enabled" : false },
            "fielddata" : { "format" : "disabled" }
          }
        }
      }, {
        "string_fields" : {
          "match" : "*",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      } ],
      "properties" : {
        "@timestamp": { "type" : "date" },
        "@version": { "type" : "string", "index" : "not_analyzed" },
        "cgroups": {"type" : "string", "index" : "not_analyzed" },
        "elapsed_clock": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "elapsed_clock_from_start": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "elapsed_time": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "elapsed_time_from_start": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "force_sync" : { "type": "boolean", "index": "not_analyzed" },
        "jams" : { "type": "boolean", "index": "not_analyzed" },
        "params_br_lat": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_br_lon": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_lat": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_lon": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_numdoc": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "params_maxage": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "params_results": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "params_skip": { "type" : "boolean", "doc_values" : true, "index" : "not_analyzed" },
        "params_tl_lat": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_tl_lon": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "params_zoom": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "params_gzipagg": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "response_code": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "retries" : { "type": "boolean", "index": "not_analyzed" },
        "status_code": { "type" : "integer", "doc_values" : true, "index" : "not_analyzed" },
        "timestamp ": { "type" : "double", "doc_values" : true, "index" : "not_analyzed" },
        "url_port": { "type": "integer", "index": "not_analyzed" }
      }
    }
  }
}
