{
  "template" : "taxi-dwh-*",
  "settings" : {
    "index.refresh_interval" : "10s",
    "index.number_of_shards" : 2,
    "index.number_of_replicas" : 1,
    "index.routing.allocation.total_shards_per_node": 1
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : false},
      "numeric_detection": false,
      "date_detection": false,
      "dynamic_templates" : [ {
        "taxi_time_fields" : {
          "match" : "*_time",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_double_fields" : {
          "match" : "*_double",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "double", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_float_fields" : {
          "match" : "*_float",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_int_fields" : {
          "match" : "*_int",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "integer", "index" : "not_analyzed"
          }
        }
      }, {
        "string_fields" : {
          "match" : "*",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "analyzed"
          }
        }
      } ],
      "properties" : {
        "@timestamp": { "type" : "date" },
        "@version": { "type" : "string", "index" : "not_analyzed" }
      }
    }
  }
}
