{
  "template" : "*yandex-taxi-*",
  "settings" : {
    "index.refresh_interval" : "10s",
    "index.number_of_shards" : 3,
    "index.routing.allocation.total_shards_per_node": 3
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : false},
      "numeric_detection": false,
      "date_detection": false,
      "dynamic_templates" : [ {
        "message_field" : {
          "match" : "message",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "analyzed",
            "norms": { "enabled" : false }
          }
        }
      }, {
        "taxi_time_fields" : {
          "match" : "*_time",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_double_fields" : {
          "match" : "*_double",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "double", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_float_fields" : {
          "match" : "*_float",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_int_fields" : {
          "match" : "*_int",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "integer", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_geo_point_fields" : {
          "match" : "*_geo_point",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "geo_point", "index" : "not_analyzed"
          }
        }
      }, {
        "boolean_fields" : {
          "match" : "*",
          "match_mapping_type" : "boolean",
          "mapping" : {
            "type" : "boolean", "doc_values" : true, "index" : "not_analyzed",
            "norms": { "enabled" : false },
            "fielddata" : { "format" : "disabled" }
          }
        }
      }, {
        "string_fields" : {
          "match" : "*",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      } ],
      "properties" : {
        "@timestamp": { "type" : "date" },
        "@version": { "type" : "string", "index" : "not_analyzed" },
        "acceptlang": {"type" : "string", "index" : "not_analyzed" },
        "application": {"type" : "string", "index" : "not_analyzed" },
        "auth_get_uid_info_time": {"type" : "float", "index" : "not_analyzed" },
        "auth_mongo_queries_time": {"type" : "float", "index" : "not_analyzed" },
        "body": {"type" : "string", "index" : "analyzed" },
        "build_active_banners": {"type" : "float", "index" : "not_analyzed" },
        "build_active_promotions": {"type" : "float", "index" : "not_analyzed" },
        "build_data_time": {"type" : "float", "index" : "not_analyzed" },
        "build_response": {"type" : "float", "index" : "not_analyzed" },
        "build_response_time": {"type" : "float", "index" : "not_analyzed" },
        "cgroups": {"type" : "string", "index" : "not_analyzed" },
        "city_id": {"type" : "string", "index" : "not_analyzed" },
        "completed_with_card": {"type" : "integer", "index" : "not_analyzed" },
        "delay": {"type" : "float", "index" : "not_analyzed" },
        "device_id": {"type" : "string", "index" : "not_analyzed" },
        "disconnected": {"type" : "boolean", "index" : "not_analyzed" },
        "error": {"type" : "string", "index" : "not_analyzed" },
        "exc_info": {"type" : "string", "index" : "not_analyzed" },
        "fetch_city_time": {"type" : "float", "index" : "not_analyzed" },
        "fetch_driver_pos_time": {"type" : "float", "index" : "not_analyzed" },
        "fetch_phone_country_park_driver_route_positions_time": {"type" : "float", "index" : "not_analyzed" },
        "fetch_user_proc_order_time": {"type" : "float", "index" : "not_analyzed" },
        "host": {"type" : "string", "index" : "not_analyzed" },
        "level": {"type" : "string", "index" : "not_analyzed" },
        "link": {"type" : "string", "index" : "not_analyzed" },
        "meta": {"type" : "string", "index" : "not_analyzed" },
        "meta_code": {"type" : "integer", "index" : "not_analyzed" },
        "meta_driver_id": {"type" : "string", "index" : "not_analyzed" },
        "meta_order_id": {"type" : "string", "index" : "not_analyzed" },
        "meta_park_id": {"type" : "string", "index" : "not_analyzed" },
        "meta_taxi_alias_id": {"type" : "string", "index" : "not_analyzed" },
        "meta_type": {"type" : "string", "index" : "not_analyzed" },
        "meta_user_id": {"type" : "string", "index" : "not_analyzed" },
        "meta_user_phone": {"type" : "string", "index" : "not_analyzed" },
        "meta_user_uid": {"type" : "string", "index" : "not_analyzed" },
        "metadata": {"type" : "string", "index" : "not_analyzed" },
        "method": {"type" : "string", "index" : "not_analyzed" },
        "min_card_orders": {"type" : "integer", "index" : "not_analyzed" },
        "module": {"type" : "string", "index" : "not_analyzed" },
        "order_id": {"type" : "string", "index" : "not_analyzed" },
        "orders_mongo_queries_time": {"type" : "float", "index" : "not_analyzed" },
        "other_data_time": {"type" : "float", "index" : "not_analyzed" },
        "overdue": {"type" : "float", "index" : "not_analyzed" },
        "parse_args": {"type" : "float", "index" : "not_analyzed" },
        "parse_request_time": {"type" : "float", "index" : "not_analyzed" },
        "passed_user_id": {"type" : "string", "index" : "not_analyzed" },
        "path": {"type" : "string", "index" : "not_analyzed" },
        "phone_id": {"type" : "string", "index" : "not_analyzed" },
        "port": {"type" : "string", "index" : "not_analyzed" },
        "reason": {"type" : "string", "index" : "not_analyzed" },
        "source": {"type" : "string", "index" : "not_analyzed" },
        "start_timestamp": {"type" : "float", "index" : "not_analyzed" },
        "stopwatch_name": {"type" : "string", "index" : "not_analyzed" },
        "stopwatch_units": {"type" : "string", "index" : "not_analyzed" },
        "sync_proc_with_order_time": {"type" : "float", "index" : "not_analyzed" },
        "surge_pins": {"type" : "integer", "index" : "not_analyzed" },
        "surge_pins_diff": {"type" : "integer", "index" : "not_analyzed" },
        "surge_free": {"type" : "integer", "index" : "not_analyzed" },
        "surge_free_diff": {"type" : "integer", "index" : "not_analyzed" },
        "surge_total": {"type" : "integer", "index" : "not_analyzed" },
        "surge_total_diff": {"type" : "integer", "index" : "not_analyzed" },
        "surge_radius": {"type" : "double", "index" : "not_analyzed" },
        "surge_radius_diff": {"type" : "double", "index" : "not_analyzed" },
        "surge_tl": {"type" : "geo_point", "index" : "not_analyzed" },
        "surge_tl_diff_lon": {"type" : "double", "index" : "not_analyzed" },
        "surge_tl_diff_lat": {"type" : "double", "index" : "not_analyzed" },
        "surge_br": {"type" : "geo_point", "index" : "not_analyzed" },
        "surge_br_diff_lon": {"type" : "double", "index" : "not_analyzed" },
        "surge_br_diff_lat": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_econom": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_comfort": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_comfortplus": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_business": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_minivan": {"type" : "double", "index" : "not_analyzed" },
        "surge_v_diff_vip": {"type" : "double", "index" : "not_analyzed" },
        "text": {"type" : "string", "index" : "analyzed" },
        "total_time": {"type" : "float", "index" : "not_analyzed" },
        "type": {"type" : "string", "index" : "not_analyzed" },
        "update_user_banners": {"type" : "float", "index" : "not_analyzed" },
        "uri": {"type" : "string", "index" : "not_analyzed" },
        "url": {"type" : "string", "index" : "not_analyzed" },
        "user_uid": {"type" : "string", "index" : "not_analyzed" },
        "useragent": {"type" : "string", "index" : "not_analyzed" },
        "yandex_uid": {"type" : "string", "index" : "not_analyzed" },
        "yandex_uuid": {"type" : "string", "index" : "not_analyzed" },
        "geoip"  : {
          "dynamic": true,
          "properties" : {
            "ip": { "type": "ip" },
            "location" : { "type" : "geo_point" },
            "latitude" : { "type" : "float" },
            "longitude" : { "type" : "float" }
          }
        }
      }
    }
  }
}
