{
  "template" : "*taximeter-logs-*",
  "settings" : {
    "index.refresh_interval" : "10s",
    "index.number_of_shards" : 3,
    "index.number_of_replicas" : 1,
    "index.routing.allocation.total_shards_per_node": 3
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : false},
      "numeric_detection": false,
      "date_detection": false,
      "dynamic_templates" : [ {
        "message_field" : {
          "match" : "message",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      }, {
        "taxi_time_fields" : {
          "match" : "*_time",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_double_fields" : {
          "match" : "*_double",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "double", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_float_fields" : {
          "match" : "*_float",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "float", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_int_fields" : {
          "match" : "*_int",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "integer", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_geo_point_fields" : {
          "match" : "*_geo_point",
          "match_mapping_type" : "*",
          "mapping" : {
            "type" : "geo_point", "index" : "not_analyzed"
          }
        }
      }, {
        "taxi_boolean_fields" : {
          "match" : "*_bool",
          "match_mapping_type" : "boolean",
          "mapping" : {
            "type" : "boolean", "doc_values" : true, "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      }, {
        "string_fields" : {
          "match" : "*",
          "match_mapping_type" : "string",
          "mapping" : {
            "type" : "string", "index" : "not_analyzed",
            "norms": { "enabled" : false }
          }
        }
      } ],
      "properties" : {
        "@timestamp": { "type" : "date" },
        "@version": { "type" : "string", "index" : "not_analyzed" },
        "cgroups": {"type" : "string", "index" : "not_analyzed" },
        "log_module": {"type" : "string", "index" : "not_analyzed" },
        "log_level": {"type" : "string", "index" : "not_analyzed" },
        "log_message": {"type" : "string", "index" : "analyzed" },
        "meta_request_path": {"type" : "string", "index" : "analyzed" },
        "meta_request_url": {"type" : "string", "index" : "analyzed" },
        "meta_DataCToE": {"type" : "float", "index" : "not_analyzed" },
        "meta_DataCmds": {"type" : "float", "index" : "not_analyzed" },
        "meta_DataEtoS": {"type" : "float", "index" : "not_analyzed" },
        "meta_DataSToR": {"type" : "float", "index" : "not_analyzed" },
        "meta_LogCToE": {"type" : "float", "index" : "not_analyzed" },
        "meta_LogCmds": {"type" : "float", "index" : "not_analyzed" },
        "meta_LogEtoS": {"type" : "float", "index" : "not_analyzed" },
        "meta_LogSToR": {"type" : "float", "index" : "not_analyzed" },
        "meta_ResponseSize": {"type" : "float", "index" : "not_analyzed" },
        "meta_TempCToE": {"type" : "float", "index" : "not_analyzed" },
        "meta_TempCmds": {"type" : "float", "index" : "not_analyzed" },
        "meta_TempEtoS": {"type" : "float", "index" : "not_analyzed" },
        "meta_TempSToR": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_CellsCount": {"type" : "integer", "index" : "not_analyzed" },
        "meta_Surge_TilesCount": {"type" : "integer", "index" : "not_analyzed" },
        "meta_Surge_RegionsCount": {"type" : "integer", "index" : "not_analyzed" },
        "meta_Surge_PinsCount": {"type" : "integer", "index" : "not_analyzed" },
        "meta_Surge_Preparation": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_SavingJson": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_SavingFlatbuffers": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_SavingMetadata": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_DistributingPinstats": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_Distribution": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_LoadingGrid": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_LoadingPinstats": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_SavingTiles": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_Rendering": {"type" : "float", "index" : "not_analyzed" },
        "meta_Surge_TotalTime": {"type" : "float", "index" : "not_analyzed" },
        "meta_ElapsedTime": {"type" : "float", "index" : "not_analyzed" }
      }
    }
  }
}
