ident: taxi

logger:
    mode: [file, stderr]
    file: /var/log/statbox/push-client-taxi-contractor-merch-payments-bot.log
    level: 4
    timeformat: "%d.%m.%Y-%H:%M:%S"

network:
    master_addr: logbroker.yandex.net
    proto: pq
    transport: ipv6

watcher:
    state: /var/lib/push-client/contractor-merch-payments-bot

files:
  - name: /var/log/yandex/taxi-contractor-merch-payments-bot-stq3/taxi.log
    log_type: taxi-contractor-merch-payments-bot-yandex-taxi-contractor-merch-payments-bot-stq3-testing-log
  - name: /var/log/yandex/taxi-contractor-merch-payments-bot-web/taxi.log
    log_type: taxi-contractor-merch-payments-bot-yandex-taxi-contractor-merch-payments-bot-web-testing-log
