{
  "ScoredOrder": {
    "has_additional_properties": false,
    "properties": {
      "best_cand_index": {
        "description": "Индекс лучшего кандидата, если остался хотя бы один",
        "type": "integer"
      },
      "candidates": {
        "description": "Все кандидаты из запроса",
        "items": {
          "schema": "ScoredCandidate",
          "x-taxi-clickable": true
        },
        "type": "array"
      },
      "candidates_count": {
        "description": "Число кандидатов отфильтрованных скорингом",
        "type": "integer"
      },
      "misc": {
        "schema": "ExtraOrderMisc",
        "x-taxi-clickable": true
      },
      "retention_score": {
        "description": "Скор удержания заказа в буфере, это поле можно менять из скрипта",
        "type": "number"
      }
    },
    "required": [
      "candidates",
      "candidates_count"
    ],
    "type": "object"
  },
  "order_contexts": {
    "description": "Контексты всех заказов запроса",
    "items": {
      "schema": "order_context",
      "x-taxi-clickable": true
    },
    "type": "array"
  }
}
