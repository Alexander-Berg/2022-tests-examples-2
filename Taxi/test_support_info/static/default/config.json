  "STARTRACK_FINANCE_TICKET_RULES": [
    {
      "conditions": {
        "country": "rus"
      },
      "email_from": "park@taxi.yandex.ru",
      "queue": "SUPPARTNERS"
    },
    {
      "conditions": {
        "#or": [
          {
            "country": {
              "#ne": "rus"
            },
            "customer_user_platform": {
