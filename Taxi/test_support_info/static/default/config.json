      },
      "email_from": "support@support-uber.com",
      "queue": "SUPPORT"
    },
    {
      "conditions": {
        "#or": [
          {
            "customer_user_platform": {
              "#in": [
                "yango_iphone",
                "yango_android"
              ]
            }
          },
          {
            "user_platform": {
              "#in": [
