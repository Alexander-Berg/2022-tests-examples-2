{
    "name": "corp-client",
    "description": "corp-client tests",
    "author": "Natalich Maksim <nenatalich@yandex-team.ru>",
    "repository": {
        "type": "git",
        "url": "https://github.yandex-team.ru/taxi/taxifrontend-tests-ui.git"
    },
    "license": "UNLICENSED",
    "scripts": {
        "cypress:unstable": "npx cypress run --headless --browser electron --config baseUrl=https://corp-client.taxi.dev.yandex.ru",
        "cypress:testing": "npx cypress run --browser electron --config baseUrl=https://corp-client.taxi.tst.yandex.ru",
        "cypress:headless": "npx cypress run --config baseUrl=https://${USER}-corp-client.front.taxi.dev.yandex.ru",
        "cypress:dev": "npx cypress open --config baseUrl=https://corp-client.${USER}.local.yandex.ru:8443/account/staff",
        "allure:report": "allure generate allure-results --clean -o cypress/artifacts/allure-report",
        "test": "npx cypress run --browser chromium ; export TESTS_CODE=$? && npm run allure:report && exit $TESTS_CODE"
    },
    "dependencies": {
        "@shelex/cypress-allure-plugin": "2.23.0",
        "allure-commandline": "2.17.2",
        "axios": "0.21.4",
        "cypress": "8.3.1",
        "cypress-file-upload": "4.0.7",
        "cypress-nhover": "1.0.0",
        "cypress-xpath": "1.6.0",
        "dayjs": "1.10.6"
    }
}
