{
  "nlu": {
    "preprocessor": {
      "normalizer": {
        "enabled": false
      },
      "text_replacer": {
        "replacements": [
          [
            "m",
            "м"
          ],
          [
            "b",
            "б"
          ],
          [
            "v",
            "в"
          ],
          [
            "t",
            "т"
          ],
          [
            "!!!",
            "."
          ],
          [
            "\\d+",
            "ЧИСЛО"
          ],
          [
            "\\s+",
            "_"
          ]
        ]
      }
    }
  }
}
