{
  "general": {
    "available_languages": [
      "en",
      "fr"
    ],
    "lang": "en"
  },
  "nlu": {
    "preprocessor": {
      "features_mapping": {
        "task_language": "language"
      },
      "language_detector": {
        "enabled": true,
        "parameters": {
          "hints": [
            "en",
            "fr"
          ]
        },
        "type": "yandex"
      },
      "normalizer": {
        "enabled": false
      },
      "speller": {
        "enabled": true,
        "parameters": {
          "languages": [
            "en",
            "fr"
          ]
        },
        "type": "yandex"
      },
      "text_filter": {
        "filter_html": true,
        "regexp_extractors": [
          "описание проблемы:\\s*(.+)",
          "описание:\\s*(.+)",
          "(.*)(пять\\s)?тест"
        ],
        "to_lower": true
      },
      "translator": {
        "enabled": true,
        "parameters": {
          "target_language": "en"
        },
        "type": "yandex"
      }
    }
  }
}
