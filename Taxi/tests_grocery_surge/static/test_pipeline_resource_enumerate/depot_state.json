{
  "instance_schema": {
    "additionalProperties": false,
    "properties": {
      "orders": {
        "description": "Список заказов по лавке",
        "items": {
          "additionalProperties": false,
          "properties": {
            "assembly_finished": {
              "description": "время завершения сборки заказа",
              "format": "date-time",
              "type": "string"
            },
            "assembly_started": {
              "description": "время начала сборки заказа",
              "format": "date-time",
              "type": "string"
            },
            "cart_id": {
              "format": "uuid",
              "type": "string"
            },
            "created": {
              "description": "время создания заказа",
              "format": "date-time",
              "type": "string"
            },
            "delivery_type": {
              "description": "Тип доставки - либо курьером, либо самовывоз, либо беспилотником",
              "enum": [
                "eats_dispatch",
                "courier",
                "pickup",
                "rover",
                "taxi"
              ],
              "type": "string"
            },
            "dispatch_status": {
              "enum": [
                "new",
                "matched",
                "dispatching",
                "dispatch_awaiting",
                "delivering",
                "handing",
                "returning"
              ],
              "type": "string"
            },
            "eta": {
              "description": "прогнозное время доставки",
              "format": "date-time",
              "type": "string"
            },
            "location": {
              "additionalProperties": false,
              "example": {
                "lat": 39.60258,
                "lon": 52.569089
              },
              "properties": {
                "lat": {
                  "description": "Широта",
                  "maximum": 90,
                  "minimum": -90,
                  "type": "number"
                },
                "lon": {
                  "description": "Долгота",
                  "maximum": 180,
                  "minimum": -180,
                  "type": "number"
                }
              },
              "required": [
                "lon",
                "lat"
              ],
              "type": "object"
            },
            "order_id": {
              "type": "string"
            },
            "order_status": {
              "enum": [
                "draft",
                "canceled",
                "checked_out",
                "reserving",
                "reserved",
                "assembling",
                "assembled",
                "delivering",
                "closed",
                "pending_cancel"
              ],
              "type": "string"
            },
            "performer_id": {
              "description": "курьер, на которого назначился заказ",
              "example": "bb5b6efc42e541f0a968b861cf41c4f0_bb291527276cea7fc4cc7364817cbcc5",
              "type": "string"
            }
          },
          "required": [
            "order_id",
            "order_status",
            "dispatch_status",
            "created",
            "location",
            "cart_id"
          ],
          "type": "object"
        },
        "type": "array"
      },
      "performers": {
        "description": "Список курьеров на лавке",
        "items": {
          "additionalProperties": false,
          "properties": {
            "eats_shift_id": {
              "description": "id едовой смены курьера",
              "example": 5578021,
              "type": "string"
            },
            "grocery_shift_id": {
              "description": "id лавочной смены курьера",
              "example": "9a6dcc1cbed9445785f59c411fa13fb5000300010000",
              "type": "string"
            },
            "performer_id": {
              "example": "bb5b6efc42e541f0a968b861cf41c4f0_bb291527276cea7fc4cc7364817cbcc5",
              "type": "string"
            },
            "performer_status": {
              "enum": [
                "idle",
                "matched",
                "pause",
                "deliver",
                "handing",
                "returning"
              ],
              "type": "string"
            },
            "position": {
              "additionalProperties": false,
              "example": {
                "lat": 39.60258,
                "lon": 52.569089
              },
              "properties": {
                "lat": {
                  "description": "Широта",
                  "maximum": 90,
                  "minimum": -90,
                  "type": "number"
                },
                "lon": {
                  "description": "Долгота",
                  "maximum": 180,
                  "minimum": -180,
                  "type": "number"
                }
              },
              "required": [
                "lon",
                "lat"
              ],
              "type": "object"
            },
            "transport_type": {
              "description": "тип транспорта",
              "example": "pedestrian, bicycle, car, etc\ntaxi (если исполнитель - не наш курьер)\n",
              "type": "string"
            }
          },
          "required": [
            "performer_id",
            "performer_status",
            "position",
            "transport_type"
          ],
          "type": "object"
        },
        "type": "array"
      }
    },
    "required": [
      "orders",
      "performers"
    ],
    "type": "object"
  },
  "is_prefetch_only": false,
  "name": "depot_state",
  "params_schema": {
    "additionalProperties": false,
    "properties": {
      "depot_id": {
        "type": "string"
      }
    },
    "required": [
      "depot_id"
    ],
    "type": "object"
  }
}
