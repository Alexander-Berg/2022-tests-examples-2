    "headers": {
        "YaTaxi-Api-Key": "@secdist.settings_override.BILLING_DOCS_API_TOKEN"
    },
    "wait": {
        "attempts": 0,
        "wait_time": 1200,
        "backoff_time": 30
    }
}
