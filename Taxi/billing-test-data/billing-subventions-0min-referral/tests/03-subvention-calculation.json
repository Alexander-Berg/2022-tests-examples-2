{
    "alias": "subventions_update_needed",
    "query": [
        {
            "json": {
                "external_obj_id": "@driver_referral_payment.json.external_obj_id",
                "kind": "subventions_update_needed"
            }
        }
    ],
    "url": "@billing_docs/v1/docs/search",
    "result": {
        "status": 200,
        "json": {
            "docs": [
                {
                    "kind": "subventions_update_needed",
                    "external_obj_id": "@json.external_obj_id",
                    "status": "complete"
                }
            ]
        }
