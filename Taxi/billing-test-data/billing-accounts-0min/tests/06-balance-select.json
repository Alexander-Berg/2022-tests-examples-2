{
  "before": "sibilla.decorators.billing_accounts.fix_balance_query",
  "dump": {
    "request": "@json",
    "response": {
      "json": "@drop_account_id(json)",
      "status": 200
    }
  },
  "headers": {
    "YaTaxi-Api-Key": "@secdist.settings_override.BILLING_ACCOUNTS_API_TOKEN"
  },
  "query": [
    {
      "json": {
        "accounts": [
          {
            "account_id": "@journal-append.json.entries.0.account_id"
          }
        ],
        "accrued_at": [
          "@journal-append.json.entries.0.event_at"
        ],
        "limit": 100,
        "offset": 0
      }
    }
  ],
  "result": {
    "status": 200
  },
  "url": "@billing_accounts/v1/balances/select"
}
