{
  "query": [
    {
      "json": {
        "accounts": [
          {
            "account_id": "@journal-append.json.account_id"
          }
        ],
        "limit": 100,
        "offset": 0,
        "accrued_at": [
          "@journal-append.json.event_at"
        ]
      }
    }
  ],
  "url": "@billing_accounts/v1/balances/select",
  "result": {
    "status": 200
  },
  "before": "sibilla.decorators.billing_accounts.fix_balance_query",
  "dump": {
    "request": "@json",
    "response": {
      "json": [
        {
          "account": {
            "entity_external_id": "@json.0.account.entity_external_id",
            "agreement_id": "@json.0.account.agreement_id",
            "currency": "@json.0.account.currency",
            "sub_account": "@json.0.account.sub_account",
            "opened": "@json.0.account.opened",
            "expired": "@json.0.account.expired"
          },
          "balances": "@json.0.balances"
        }
      ],
      "status": 200
    }
  },
  "headers": {
    "YaTaxi-Api-Key": "@secdist.settings_override.BILLING_ACCOUNTS_API_TOKEN"
  }
}