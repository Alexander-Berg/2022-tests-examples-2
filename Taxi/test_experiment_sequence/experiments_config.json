{
	"URL": "http://experiments3.taxi.tst.yandex.net",
	"TVM_TICKET": "1",
	"EXPERIMENTS": {
		"infranaim_park_choice": {
			"name": "infranaim_park_choice",
			"route": "/v1/experiments",
			"consumer": "infranaim/park_choice",
			"fields": {
				"phone": {
					"name": "phone",
					"type": "string"
				},
				"country": {
					"name": "country",
					"type": "string"
				},
				"medium": {
					"name": "medium",
					"type": "string"
				},
				"deaf_candidate": {
					"name": "deaf_candidate",
					"type": "int"
				},
				"workflow": {
					"name": "workflow",
					"type": "string"
				},
				"order_count": {
					"name": "order_count",
					"type": "int"
				},
				"source": {
					"name": "source",
					"type": "string"
				},
				"city": {
					"name": "city",
					"type": "string"
				},
				"vacancy": {
					"name": "vacancy",
					"type": "string",
					"nullable": true
				}
			},
			"history": {
				"perform_search": false,
				"days_limit": 0,
				"update_on_existing_result": {
					"value": {}
				}
			}
		},
		"infranaim_cannibalization": {
			"name": "infranaim_cannibalization",
			"route": "/v1/experiments",
			"consumer": "infranaim/cannibalization",
			"fields": {},
			"history": {
				"perform_search": true,
				"days_limit": 30,
				"auditorium": "check_all",
				"update_on_existing_result": {
					"value": {
						"ticket_schema": [
							{
								"tag": "не_лид",
								"name": "Не лид",
								"ticket_param_name": "status"
							},
							{
								"tag": "уже_работает_в_сервисе",
								"name": "уже работает в сервисе",
								"ticket_param_name": "reject_reason"
							}
						],
						"ticket_tags": [
							"experiment_cannibalization_double"
						]
					}
				}
			}
		},
		"history_members_search": {
			"name": "history_members_search",
			"route": "/v1/experiments",
			"consumer": "infranaim/history_members_search",
			"fields": {},
			"history": {
				"perform_search": true,
				"days_limit": 30,
				"auditorium":  "check_members",
				"update_on_existing_result": {
					"value": {
						"ticket_schema": [
							{
								"tag": "не_лид",
								"name": "Не лид",
								"ticket_param_name": "status"
							},
							{
								"tag": "уже_работает_в_сервисе",
								"name": "уже работает в сервисе",
								"ticket_param_name": "reject_reason"
							}
						],
						"ticket_tags": [
							"history_members_search_double"
						]
					}
				}
			}
		}
	}
