{
  "allow-unauthorized": true,
  "default-response": "resp-ok",
  "enabled": true,
  "responses": [
    {
      "body": {
        "env": "stable",
        "result#switch": {
          "cases": [
            {
              "case": 200,
              "value": "Ok"
            },
            {
              "case": 400,
              "value": "Bad request"
            },
            {
              "case": 404,
              "value": "Not found"
            },
            {
              "case": 500,
              "value": "Server error"
            }
          ],
          "default-value": "Failure",
          "input#get": {
            "key": "code",
            "object#request-body": {}
          }
        }
      },
      "content-type": "application/json",
      "id": "resp-ok"
    }
  ]
}
