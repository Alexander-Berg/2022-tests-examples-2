#!/bin/sh

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

set -e

case "$1" in
    remove)
        remove3
        remove1
        remove2
        rtest-package-name
    ;;

    purge)
        purge3
        purge1
        purge2
        ptest-package-name
    ;;

    upgrade)
        upgrade3
        upgrade1
        upgrade2
        utest-package-name
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0
