{
  "EATS_RESTAPP_MARKETING_BID_RECOMMENDER_CACHE": {
    "batch_size": 1000,
    "cluster": "yt-local",
    "enabled": false,
    "limit": 1000,
    "table": "//home/testsuite/bid_recommendations"
  },
  "EATS_RESTAPP_MARKETING_CANVAS_SETTINGS": {
    "create_creatives_batch": {
      "headers": [
        {
          "name": "Authorization",
          "prefix": "oauth "
        }
      ],
      "name": "batch",
      "url": {
        "$mockserver": "/canvas-api/html5/batch"
      }
    },
    "product_type": "GENERAL",
    "service_name": "direct",
    "upload_archive": {
      "attacment": " ",
      "headers": [
        {
          "name": "Authorization",
          "prefix": "oauth "
        },
        {
          "content": "application/json",
          "name": "Content-Type"
        }
      ],
      "url": {
        "$mockserver": "/canvas-api/html5/source"
      }
    },
    "upload_creatives": {
      "headers": [
        {
          "name": "Authorization",
          "prefix": "oauth "
        },
        {
          "content": "application/json",
          "name": "Content-Type"
        }
      ],
      "url": {
        "$mockserver": "/canvas-api/html5/direct/creatives"
      }
    }
  },
  "EATS_RESTAPP_MARKETING_FEEDS_ADMIN_SETTINGS": {
    "create_banner_priority": 100,
    "media_upload_image_parameters": "name_at_config",
    "media_upload_url": {
      "$mockserver": "/v1/media/upload"
    },
    "x_yandex_login": "robot-rest-marketing"
  }
}
