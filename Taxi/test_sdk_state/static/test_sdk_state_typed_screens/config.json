        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 5,
        "visibility_requirements": {
          "has_plus": false,
          "has_positive_balance": false,
          "required_experiment": "catching_up_cashback"
        },
        "widgets": [
          "widget:taxi:catching_up_cashback:no_balance_top_text_with_cashback",
          "widget:taxi:catching_up_cashback:text",
          "widget:taxi:catching_up_cashback"
        ]
      },
      "plaque:taxi:catching_up_cashback_no_positive_balance_success_purchase": {
        "condition": {
          "screens": [
            "summary"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 5,
        "visibility_requirements": {
          "has_plus": true,
          "has_positive_balance": false,
          "required_experiment": "catching_up_cashback"
        },
        "widgets": [
          "widget:taxi:catching_up_cashback:no_balance_top_text_with_cashback",
          "widget:taxi:catching_up_cashback:after_purchase_plus:cashback_after_ride:no_balance"
        ]
      },
      "plaque:taxi:composite_payment": {
        "condition": {
          "screens": [
            "summary"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 50,
        "visibility_requirements": {
          "has_cashback_offer": true,
          "has_plus": true,
          "has_positive_balance": true
        },
        "widgets": [
          "widget:common:balance_without_text",
          "widget:taxi:composite_payment"
        ]
      },
      "plaque:taxi:plus_burns": {
        "condition": {
          "screens": [
            "summary"
          ],
          "unseen_typed_screens": [
            "plus_burns",
            "sweet-home:burn"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 5,
        "visibility_requirements": {
          "has_cashback_offer": true,
          "has_plus": false,
          "required_experiment": "sweet-home:plaques:burn"
        },
        "widgets": [
          "widget:common:balance_without_text",
          "widget:taxi:plus_burns"
        ]
      }
    }
  },
