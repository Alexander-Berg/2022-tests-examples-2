{
  "PLUS_SWEET_HOME_PLAQUES_BY_SERVICE": {
    "__global__": {
      "plaque:global:buy_plus": {
        "condition": {
          "screens": [
            "main"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 10,
        "visibility_requirements": {
          "has_plus": false,
          "required_experiment": "no_catching_up_cashback"
        },
        "widgets": [
          "widget:common:balance_without_text",
          "widget:common:buy_plus"
        ]
      }
    },
    "taxi": {
      "plaque:taxi:catching_up_cashback_has_positive_balance": {
        "condition": {
          "screens": [
            "summary"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 5,
        "visibility_requirements": {
          "has_plus": false,
          "has_positive_balance": true,
          "required_experiment": "catching_up_cashback"
        },
        "widgets": [
          "widget:taxi:catching_up_cashback:has_balance_top_text_with_cashback",
          "widget:taxi:catching_up_cashback:text",
          "widget:taxi:catching_up_cashback"
        ]
      },
      "plaque:taxi:catching_up_cashback_has_positive_balance_success_purchase": {
        "condition": {
          "screens": [
            "summary"
          ]
        },
        "layout": "VERTICAL",
        "params": {
          "close_after": 60,
          "show_after": 10
        },
        "priority": 5,
        "visibility_requirements": {
          "has_plus": true,
          "has_positive_balance": true,
          "required_experiment": "catching_up_cashback"
        },
        "widgets": [
          "widget:common:balance_without_text",
          "widget:taxi:catching_up_cashback:after_purchase_plus:cashback_after_ride:has_balance"
        ]
