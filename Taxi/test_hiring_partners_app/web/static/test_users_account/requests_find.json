{
  "invalid_login": [
    {
      "REQUEST": {
        "body": {
          "name": "John",
          "second_name": "Doe"
        },
        "headers": {
          "X-Yandex-Login": "*&*^%$%^"
        }
      },
      "RESPONSE": {
        "body": {
          "code": "invalid_login",
          "message": "Login *&*^%$%^ can not be used"
        },
        "status_code": 400
      }
    }
  ],
  "valid": [
    {
      "REQUEST": {
        "headers": {
          "X-Yandex-Login": "ojgen"
        }
      },
      "RESPONSE": {
        "body": {
          "user": {
            "deactivated": true,
            "operation": "find",
            "user_name": "Doe John",
            "yandex_login": "ojgen"
          }
        },
        "status_code": 200
      }
    }
  ],
  "valid_non_existent": [
    {
      "REQUEST": {
        "headers": {
          "X-Yandex-Login": "ojgen"
        }
      },
      "RESPONSE": {
        "body": {
          "code": "non_existent_user",
          "message": "User ojgen does not exist"
        },
        "status_code": 404
      }
    }
  ]
}
