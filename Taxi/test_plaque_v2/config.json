  "PLUS_SWEET_HOME_INDENT_RULES": {
    "level_default_2": {
      "indent_bottom": 2,
      "indent_left": 2,
      "indent_right": 2,
      "indent_top": 2
    },
    "plaque_default_4": {
      "indent_bottom": 4,
      "indent_left": 4,
      "indent_right": 4,
      "indent_top": 4
    },
    "widget_default_4": {
      "indent_bottom": 4,
      "indent_left": 4,
      "indent_right": 4,
      "indent_top": 4
    }
  },
  "PLUS_SWEET_HOME_PLAQUES_V2_BY_SERVICE": {
    "__global__": {},
    "taxi": {
      "v2_plaque:taxi:composite_payment": {
        "condition": {
          "available_tariffs": [
            {
              "tariff": "econom",
              "vertical": "ultima"
            }
          ],
          "payment_methods": [
            "cash",
            "card"
          ],
          "screens": [
            "summary"
          ],
          "selected_tariffs": [
            {
              "tariff": "econom",
              "vertical": "ultima"
            }
          ],
          "tariffs": [
            "econom"
          ]
        },
        "display_rules": {
          "background_color_settings_id": "default_gradient",
          "background_shape_settings": "default_shape_settings",
          "indent_rules_id": "plaque_default_4"
        },
        "params": {
          "close_after": 60,
          "show_after": 0
        },
        "priority": 50,
        "visibility_requirements": {
          "has_plus": true,
          "has_positive_balance": true
        },
        "widgets_level_id": [
          "balance_level",
          "composite_payment_level"
        ]
      }
    }
  },
  "PLUS_SWEET_HOME_PLAQUE_V2_WIDGETS": {
    "widget:common:balance": {
      "action": {
        "tariff": "econom",
        "type": "SWITCH_TARIFF",
        "vertical": "ultima"
      },
      "balance": {
        "balance_key": "sweet_home.plaque.widgets.balance.text"
      },
      "content_description": {
        "text_key": "sweet_home.plaque.widgets.balance.content_description.text"
      },
      "display_widget_rules": {
        "display_rules": {
          "background_color_settings_id": "transparent",
          "background_shape_settings": "default_shape_settings",
          "indent_rules_id": "widget_default_4"
        },
        "type": "fit"
      },
      "type": "BALANCE"
    },
    "widget:taxi:composite_payment": {
      "action": {
        "setting_id": "composite_payment.enabled",
        "type": "SETTING"
      },
      "display_widget_rules": {
        "display_rules": {
          "background_color_settings_id": "transparent",
          "background_shape_settings": "default_shape_settings",
          "indent_rules_id": "widget_default_4"
        },
        "type": "fit"
      },
      "switch": {
        "text_key": "sweet_home.plaque.widgets.composite_payment.text"
      },
      "templates": [
        "$$COMPOSITE_PAYMENT_AMOUNT$$"
      ],
      "type": "SWITCH"
    }
  },
  "PLUS_SWEET_HOME_SHAPE_SETTINGS": {
    "default_shape_settings": {
      "left_bottom_corner": {
        "height_fix": 10,
        "type": "fix"
      },
      "left_top_corner": {
        "height_fix": 10,
        "type": "fix"
      },
      "right_bottom_corner": {
        "type": "half_height"
      },
      "right_top_corner": {
        "height_fix": 10,
        "type": "fix"
      }
    }
  },
  "PLUS_SWEET_HOME_WIDGETS_LEVEL": {
    "balance_level": {
      "display_rules": {
        "background_color_settings_id": "transparent",
        "background_shape_settings": "default_shape_settings",
        "indent_rules_id": "level_default_2"
      },
      "elements": [
        {
          "type": "widget",
          "widget_id": "widget:common:balance"
        }
      ]
    },
    "composite_payment_level": {
      "display_rules": {
        "background_color_settings_id": "transparent",
        "background_shape_settings": "default_shape_settings",
        "indent_rules_id": "level_default_2"
      },
      "elements": [
        {
          "type": "widget_group",
          "widget_group_id": "default_widget_group"
        }
      ]
    }
  },
  "PLUS_SWEET_HOME_WIDGET_GROUPS": {
    "default_widget_group": {
      "display_rules": {
        "background_color_settings_id": "default_gradient",
        "background_shape_settings": "default_shape_settings",
        "indent_rules_id": "plaque_default_4"
      },
      "widgets": [
        "widget:taxi:composite_payment"
      ]
    }
  }
