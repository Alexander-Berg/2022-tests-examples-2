{
  "aliases": {},
  "index_patterns": [
    "*yandex-taxi-*"
  ],
  "mappings": {
    "date_detection": false,
    "dynamic_templates": [
      {
        "message_field": {
          "mapping": {
            "norms": false,
            "type": "text"
          },
          "match": "message",
          "match_mapping_type": "string"
        }
      },
      {
        "taxi_time_fields": {
          "mapping": {
            "type": "float"
          },
          "match": "*_time",
          "match_mapping_type": "*"
        }
      },
      {
        "taxi_get_usages_time_fields": {
          "mapping": {
            "index": true,
            "type": "float"
          },
          "match": "get_usages_*_time",
          "match_mapping_type": "*"
        }
      }
    ],
    "numeric_detection": false,
    "properties": {
      "cargo_ref_id": {
        "type": "keyword"
      },
      "cgroups": {
        "type": "keyword"
      },
      "corp_client_id": {
        "type": "keyword"
      },
      "device_id": {
        "type": "keyword"
      },
      "level": {
        "type": "keyword"
      },
      "meta_code": {
        "coerce": true,
        "type": "integer"
      },
      "meta_driver_id": {
        "type": "keyword"
      },
      "meta_order_id": {
        "type": "keyword"
      },
      "meta_park_id": {
        "type": "keyword"
      },
      "meta_type": {
        "type": "keyword"
      },
      "meta_user_id": {
        "type": "keyword"
      },
      "meta_user_uid": {
        "type": "keyword"
      },
      "method": {
        "type": "keyword"
      },
      "ngroups": {
        "type": "keyword"
      },
      "phone_id": {
        "type": "keyword"
      },
      "queue": {
        "type": "keyword"
      },
      "requested_uri": {
        "type": "keyword"
      },
      "requested_with": {
        "type": "keyword"
      },
      "trace_id": {
        "type": "keyword"
      },
      "tvm_source": {
        "type": "keyword"
      },
      "useragent": {
        "type": "text"
      }
    }
  },
  "order": 0,
  "settings": {
    "index": {
      "lifecycle": {
        "name": "delete-7days-old"
      },
      "mapping": {
        "total_fields": {
          "limit": "5000"
        }
      },
      "number_of_replicas": "1",
      "number_of_shards": "3",
      "refresh_interval": "60s",
      "routing": {
        "allocation": {
          "total_shards_per_node": "2"
        }
      }
    }
  }
}
