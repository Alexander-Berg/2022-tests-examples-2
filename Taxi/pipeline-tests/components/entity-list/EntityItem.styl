.EfficiencyPipelineAlgorithmsTestsEntityList {
    &__row {
        background-color: $MainWhite;
        height: $unit * 8;
        padding: 0 $unit * 2;
        cursor: pointer;

        &:hover {
            background-color: $WarmGray150;
        }

        &_isDarkRow {
            background-color: $WarmGray100;
        }
    }

    &__subtitle {
        color: $WarmGray300;
        font-style: italic;
    }
}
