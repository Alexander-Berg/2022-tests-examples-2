#!/bin/sh

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

set -e

case "$1" in
    remove)
    ;;

    purge)
        
        if dpkg-statoverride --list /var/log/yandex/taxi-test-service >/dev/null 2>&1; then
            dpkg-statoverride --remove /var/log/yandex/taxi-test-service
        fi

    ;;

    upgrade)
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0
