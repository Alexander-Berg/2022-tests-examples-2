{
  "features": [
    {
      "geometry": {
        "coordinates": {
          "$var": "point"
        },
        "type": "Point"
      },
      "properties": {
        "GeocoderMetaData": {
          "Address": {
            "Components": [
              {
                "kind": "country",
                "name": {
                  "$default": "Russia",
                  "$var": "country_name"
                }
              },
              {
                "kind": "province",
                "name": "MO"
              },
              {
                "kind": "locality",
                "name": {
                  "$default": "Moscow",
                  "$var": "city"
                }
              },
              {
                "kind": "street",
                "name": "Point"
              },
              {
                "kind": "house",
                "name": "1"
              }
            ],
            "country_code": {
              "$default": "RU",
              "$var": "country_code"
            }
          },
          "kind": "house",
          "precision": "exact",
          "text": {
            "$var": "full_text"
          }
        },
        "description": {
          "$var": "description"
        },
        "name": {
          "$var": "short_text"
        }
      },
      "type": "Feature"
    }
  ],
  "type": "FeatureCollection"
}
