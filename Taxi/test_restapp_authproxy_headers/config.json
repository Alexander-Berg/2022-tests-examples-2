{
  "EATS_RESTAPP_AUTHORIZER_ROLES_PERMISSIONS_SETTINGS": {
    "roles": [
      {
        "name": "ROLE_OPERATOR",
        "permissions": [
          "permission.restaurant.functionality",
          "permission.communications.read"
        ]
      },
      {
        "name": "ROLE_MANAGER",
        "permissions": [
          "permission.restaurant.menu",
          "permission.orders.active"
        ]
      }
    ]
  },
  "EATS_RESTAPP_AUTHPROXY_PARTNER_INFO_CACHE_SETTINGS": {
    "lifetime": 60000
  },
  "EATS_RESTAPP_AUTHPROXY_PREFIX_ACCESS_RULES": [
    {
      "access_rules": [
        {
          "access_conditions": {
            "any_of": [
              {
                "all_of": [
                  {
                    "name": "orders_common",
                    "type": "permission"
                  }
                ]
              }
            ]
          },
          "check_restrictions": false,
          "method": "POST",
          "path": "/4.0/restapp-front/api/v1/client/orders/search"
        }
      ],
      "prefix": "/4.0/restapp-front/api/v1/client/orders"
    }
  ],
  "EATS_RESTAPP_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
        "http-path-prefix": "/4.0/restapp-front/api/v1/client/orders"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {}
    }
  ]
}
