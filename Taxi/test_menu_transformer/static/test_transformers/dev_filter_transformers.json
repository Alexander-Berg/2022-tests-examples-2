{
  "menu": {
    "content": {
      "filters": {
        "capitalize_first_letter_filter": {
          "class": "CapitalizeFirstLetterFilter"
        },
        "html_tags_filter": {
          "class": "HtmlTagsFilter"
        }
      },
      "services": {
        "transform_menu_service": {
          "arguments": [
            "basic_name_transformer",
            "basic_description_transformer",
            "basic_category_name_transformer",
            "CategoryImageTransformer"
          ],
          "class": "TransformMenuService"
        }
      },
      "transformers": {
        "CategoryImageTransformer": {
          "arguments": {
            "412227": "https://eda.yandex.ru/images/2794391/e2251ca0a5066912295f2fd986abc3dd.jpg",
            "412721": "https://eda.yandex.ru/images/3667559/593f377ca7540035008d4dcd285038a1.jpg"
          },
          "class": "CategoryImageTransformer"
        },
        "basic_category_name_transformer": {
          "arguments": [
            "html_tags_filter",
            "capitalize_first_letter_filter"
          ],
          "class": "CategoryNameTransformer"
        },
        "basic_description_transformer": {
          "arguments": [
            "html_tags_filter"
          ],
          "class": "DescriptionTransformer"
        },
        "basic_name_transformer": {
          "arguments": [
            "html_tags_filter",
            "capitalize_first_letter_filter"
          ],
          "class": "NameTransformer"
        }
      }
    }
  }
}
