{
  "build": {
    "CODEGEN": {
      "add-result": [
        ".yaml",
        ".json"
      ],
      "targets": [
        "taxi/uservices/services/test-service/deploy"
      ]
    },
    "SERVICE": {
      "targets": [
        "taxi/uservices/services/test-service"
      ]
    }
  },
  "data": [
    {
      "destination": {
        "path": "/app/conf/test-service/config.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/config.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/taxi_config_fallback.json"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/taxi_config_fallback.json",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/configs/config_vars.production.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/configs/config_vars.production.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/configs/config_vars.testing.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/configs/config_vars.testing.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/configs/config_vars.stress.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/configs/config_vars.stress.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/configs/config_vars.unstable.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/configs/config_vars.unstable.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/configs/config_vars.testsuite.yaml"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/configs/config_vars.testsuite.yaml",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/app/bin/my_lovely_binary_name"
      },
      "source": {
        "build_key": "SERVICE",
        "path": "taxi/uservices/services/test-service/yandex-taxi-test-service",
        "type": "BUILD_OUTPUT"
      }
    },
    {
      "destination": {
        "path": "/etc/logrotate.d/test-service"
      },
      "source": {
        "path": "taxi/uservices/services/test-service/deploy/logrotate.conf",
        "type": "ARCADIA"
      }
    },
    {
      "destination": {
        "path": "/app/conf/test-service/taxi_config_cache.json"
      },
      "source": {
        "build_key": "CODEGEN",
        "path": "{package_root}/build/services/test-service/taxi_config_fallback.json",
        "type": "BUILD_OUTPUT"
      }
    }
  ],
  "include": [
    "market/search/test-projects/my-package.json"
  ],
  "meta": {
    "description": "Shine bright like a diamond!",
    "maintainer": "me",
    "name": "test-service",
    "version": "{revision}"
  }
}
