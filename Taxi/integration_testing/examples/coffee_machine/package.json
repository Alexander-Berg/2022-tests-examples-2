{
    "meta": {
        "name": "coffee-machine",
        "maintainer": "Bulat Gabbasov <bgabbasov@yandex-team.ru>",
        "description": "coffee machine",
        "version": "latest"
    },
    "build": {
        "targets": [
            "taxi/integration_testing/examples/coffee_machine/web"
        ],
        "target-platforms": [ "default-linux-x86_64" ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "taxi/integration_testing/examples/coffee_machine/web"
            },
            "destination": {
                "path": "/coffee-machine"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}
