{
  "configs": [
    {
      "clauses": [],
      "default_value": {
        "battery_levels": {
          "low": 10,
          "sufficient": 15
        },
        "enabled": true,
        "events": {
          "last_payment_failed": {
            "action": "disable_acceleration",
            "car_tag": "disable_acceleration_car_tag",
            "user_tag": "disable_acceleration_user_tag"
          },
          "last_payment_succeeded": {
            "action": "enable_acceleration",
            "car_tag": "enable_acceleration_car_tag",
            "user_tag": "enable_acceleration_user_tag"
          },
          "low_battery_level": {
            "action": "disable_acceleration",
            "car_tag": "disable_acceleration_car_tag",
            "user_tag": "disable_acceleration_user_tag"
          },
          "scooter_in_forbidden_zone": {
            "action": "disable_acceleration",
            "car_tag": "disable_acceleration_car_tag",
            "user_tag": "disable_acceleration_user_tag"
          },
          "scooter_out_of_forbidden_zone": {
            "action": "enable_acceleration",
            "car_tag": "enable_acceleration_car_tag",
            "user_tag": "enable_acceleration_user_tag"
          },
          "sufficient_battery_level": {
            "action": "enable_acceleration",
            "car_tag": "enable_acceleration_car_tag",
            "user_tag": "enable_acceleration_user_tag"
          }
        },
        "failed_payment_statuses": [
          "no_funds"
        ],
        "forbidden_zone_tags": [
          "area_for_bad"
        ]
      },
      "match": {
        "consumers": [
          {
            "name": "scooters-misc/events_handler"
          }
        ],
        "enabled": true,
        "predicate": {
          "type": "true"
        }
      },
      "name": "scooters_events_handler_settings"
    }
  ]
}
