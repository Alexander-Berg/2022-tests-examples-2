{
  "test": {
    "definitions": {
      "data-sinks": {
        "pp-test-discard-positions": {
          "config": {
            "address": "channel:test:discard-positions",
            "channel-type": "positions",
            "protocol": "positions",
            "redis-name": "taxi-passing"
          },
          "type": "geobus"
        },
        "pp-test-pedestrian-positions": {
          "config": {
            "address": "channel:test:pedestrian-positions",
            "channel-type": "positions",
            "protocol": "universal_signal",
            "redis-name": "taxi-passing"
          },
          "type": "geobus"
        },
        "pp-test-positions": {
          "config": {
            "address": "channel:test:positions",
            "channel-type": "positions",
            "protocol": "universal_signal",
            "redis-name": "taxi-passing"
          },
          "type": "geobus"
        }
      },
      "data-streams": {
        "pp-test-prediction": {
          "partitions": 1,
          "protocol": "positions"
        }
      },
      "predicates": {
        "pedestrian": {
          "if_absent": false,
          "sensor": "transport_type",
          "type": "sensor-predicate",
          "values": [
            "pedestrian"
          ]
        },
        "signal-verified": {
          "operation": "include",
          "type": "signal-source-predicate",
          "values": [
            "Verified"
          ]
        }
      },
      "processors": {
        "time-shift-5-15": {
          "begin": 5,
          "end": 15,
          "operation": "include",
          "type": "time-shift-processor"
        }
      }
    },
    "receiver": {
      "destinations": {
        "default": {
          "archive": false,
          "data-sinks": [
            "pp-test-positions"
          ],
          "filters": [
            {
              "predicates": {
                "predicates": [
                  {
                    "ref": "pedestrian",
                    "type": "ref"
                  }
                ]
              },
              "type": "none_of"
            }
          ]
        },
        "discard-by-time": {
          "archive": false,
          "data-sinks": [
            "pp-test-discard-positions"
          ]
        },
        "pedestrian": {
          "archive": false,
          "data-sinks": [
            "pp-test-pedestrian-positions"
          ],
          "filters": [
            {
              "ref": "pedestrian",
              "type": "ref"
            }
          ]
        },
        "predictions": {
          "archive": false,
          "data-streams": [
            "pp-test-prediction"
          ],
          "processors": [
            {
              "ref": "time-shift-5-15",
              "type": "ref"
            }
          ]
        }
      }
    }
  }
}
