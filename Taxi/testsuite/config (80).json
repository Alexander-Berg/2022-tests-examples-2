{
  "EDC_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
        "http-path-prefix": "/4.0/route401"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true,
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/cookie"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "cookie-enabled": true,
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/scope"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "cookie-enabled": true,
        "passport-scopes": [
          "scope"
        ],
        "server-hosts": [
          "*"
        ]
      }
    }
  ],
  "PASSPORT_BB_URL": {
    "$mockserver": "/blackbox"
  }
}
