{
  "FLEET_ACCESS_CONTROL_ENABLED": true,
  "FLEET_ACCESS_CONTROL_MULTIFACTOR_AUTH": {
    "is_enabled": true,
    "phone_confirmation_ttl": 10
  },
  "FLEET_AUTHPROXY_ACCESS_RULES": [
    {
      "access_rules": [
        {
          "any_of_permissions": [
            "one"
          ],
          "forwarded_permissions": [
            "one",
            "two",
            "three"
          ],
          "method": "POST",
          "path": "/fleet/v1/auth/park-id-required"
        }
      ],
      "prefix": "/fleet/v1/auth/park-id-required"
    },
    {
      "access_rules": [
        {
          "method": "POST",
          "path": "/fleet/v1/auth"
        }
      ],
      "prefix": "/fleet/v1/auth"
    }
  ],
  "FLEET_AUTHPROXY_CORS": {
    "allowed-headers": [],
    "allowed-origins": [
      "localhost"
    ],
    "cache-max-age-seconds": 60
  },
  "FLEET_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
        "http-path-prefix": "/fleet/v1/noauth"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "park-id-required": false,
        "proxy-401": true,
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/fleet/v1/auth"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "park-id-required": false,
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/fleet/v1/noauth/park-id-required"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true,
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/fleet/v1/auth/park-id-required"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/fleet/v1/notconfigured"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "server-hosts": [
          "*"
        ]
      }
    }
  ],
  "PASSPORT_BB_URL": {
    "$mockserver": "/blackbox"
  },
  "PASSPORT_BB_YANDEX_TEAM_URL": {
    "$mockserver": "/blackbox-team"
  }
}
