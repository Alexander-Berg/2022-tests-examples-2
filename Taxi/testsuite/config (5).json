{
  "B2B_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
        "http-path-prefix": "/v1/cargo/"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "passport-scopes": [
          "corptaxi:all"
        ],
        "server-hosts": [
          "*"
        ]
      }
    },
    {
      "input": {
        "http-path-prefix": "/v1/cargo/proxy_401/"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true,
        "server-hosts": [
          "*"
        ]
      }
    }
  ],
  "CARGO_B2B_ACCESS_CONTROL_ENABLED": true,
  "CARGO_B2B_AUTHPROXY_ACCESS_RULES": [
    {
      "access_rules": [
        {
          "auth_mode": {
            "kind": "no_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/proxy_401/no_auth"
        },
        {
          "auth_mode": {
            "kind": "ya_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/proxy_401/ya_auth"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner",
              "owner_robot"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/proxy_401/owner_only"
        }
      ],
      "prefix": "/v1/cargo/proxy_401/"
    },
    {
      "access_rules": [
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner",
              "owner_robot"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/corp_auth/owner_only"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/corp_auth/known/*"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/corp_auth/known/*/"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/corp_auth/known/*/tail"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/*/replace/*"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/*/replace/*/"
        },
        {
          "auth_mode": {
            "any_of_permissions": [
              "owner"
            ],
            "kind": "corp_auth"
          },
          "method": "POST",
          "path": "/v1/cargo/*/replace/*/tail"
        }
      ],
      "prefix": "/v1/cargo/"
    }
  ],
  "PASSPORT_BB_URL": {
    "$mockserver": "/blackbox"
  }
}
