{
  "aliases": {},
  "index_patterns": [
    "*yandex-taxi-*"
  ],
  "mappings": {
    "date_detection": false,
    "dynamic_templates": [
      {
        "message_field": {
          "mapping": {
            "norms": false,
            "type": "text"
          },
          "match": "message",
          "match_mapping_type": "string"
        }
      },
      {
        "taxi_time_fields": {
          "mapping": {
            "type": "float"
          },
          "match": "*_time",
          "match_mapping_type": "*"
        }
      }
    ],
    "numeric_detection": false,
    "properties": {
      "meta_driver_id": {
        "type": "text"
      },
      "some_field": {
        "type": "keyword"
      }
    }
  },
  "order": 0,
  "settings": {
    "index": {
      "lifecycle": {
        "name": "delete-7days-old"
      },
      "mapping": {
        "total_fields": {
          "limit": "5000"
        }
      },
      "number_of_replicas": "1",
      "number_of_shards": "3",
      "refresh_interval": "60s",
      "routing": {
        "allocation": {
          "total_shards_per_node": "2"
        }
      }
    }
  }
}
