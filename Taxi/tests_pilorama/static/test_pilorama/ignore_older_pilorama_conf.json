{
  "file": {
    "discover_interval": "500ms",
    "start_from_begin": true
  },
  "filter": {
    "defaults": [
      {
        "key": "type",
        "value": "log"
      }
    ],
    "ignore_lines_older_than": "3d",
    "removals": [
      "host"
    ],
    "renames": [
      {
        "from": "_type",
        "to": "type"
      }
    ]
  },
  "output": {
    "elastic_version": 7,
    "hosts": [
      {
        "$mockserver": "/elasticsearch/_bulk"
      }
    ],
    "id": "my_output",
    "index": "yandex-taxi-%Y.%m.%d.%H"
  }
}
