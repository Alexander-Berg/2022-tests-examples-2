{
  "file": {
    "start_from_begin": true
  },
  "filter": {
    "defaults": [
      {
        "key": "type",
        "value": "log"
      }
    ],
    "input_format": "tskv-jaeger-span",
    "removals": [
      "host"
    ],
    "renames": []
  },
  "output": {
    "format": "jaeger-collector",
    "hosts": [
      "localhost:14250/api/v2/spans"
    ],
    "id": "my_output",
    "index": "yandex-taxi-%Y.%m.%d.%H"
  }
}
