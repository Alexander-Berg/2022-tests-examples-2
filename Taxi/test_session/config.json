{
  "EATS_RESTAPP_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
        "http-path-prefix": "/auth"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {}
    },
    {
      "input": {
        "http-path-prefix": "/noauth"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/restapp-front/noauth"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/restapp-front/some-path/noauth",
        "trim-prefix": false
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true
      }
    },
    {
      "input": {
        "http-path-prefix": "/4.0/restapp-front/test-proxy-401"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {
        "proxy-401": true
      }
    }
  ]
}
