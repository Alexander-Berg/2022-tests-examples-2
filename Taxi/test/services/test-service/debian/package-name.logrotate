/var/log/yandex/taxi-test-service/*.log {
    hourly
    missingok
    maxsize 6G
    nocompress
    rotate 1
    notifempty
    postrotate
        supervisorctl signal USR1 test-service:* ||:
    endscript
}
/var/log/yandex/taxi-test-service/exp3/exp3-matched.log {
    hourly
    missingok
    maxsize 1G
    nocompress
    rotate 1
    notifempty
    postrotate
        supervisorctl signal USR1 test-service:* ||:
    endscript
}
