{
  "ABT_FORMATTING_RULES": {
    "default_params": {
      "round": 3
    },
    "rules": [
      {
        "cond": {
          "gte": "1_000_000",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "divide_by": "1_000_000",
          "round": 1,
          "sign": true,
          "suffix": "M"
        }
      },
      {
        "cond": {
          "gte": "1_000_000"
        },
        "params": {
          "divide_by": "1_000_000",
          "round": 1,
          "suffix": "M"
        }
      },
      {
        "cond": {
          "gte": "100_000",
          "lt": "1_000_000",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "divide_by": "1_000",
          "round": 0,
          "sign": true,
          "suffix": "K"
        }
      },
      {
        "cond": {
          "gte": "100_000",
          "lt": "1_000_000"
        },
        "params": {
          "divide_by": "1_000",
          "round": 0,
          "suffix": "K"
        }
      },
      {
        "cond": {
          "gte": "1_000",
          "lt": "100_000",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "divide_by": "1_000",
          "round": 1,
          "sign": true,
          "suffix": "K"
        }
      },
      {
        "cond": {
          "gte": "1_000",
          "lt": "100_000"
        },
        "params": {
          "divide_by": "1_000",
          "round": 1,
          "suffix": "K"
        }
      },
      {
        "cond": {
          "gte": "0",
          "lt": "1000",
          "types": [
            "relative_diff"
          ]
        },
        "params": {
          "round": 2,
          "sign": true,
          "suffix": "%"
        }
      },
      {
        "cond": {
          "gte": "100",
          "lt": "1000",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "round": 2,
          "sign": true
        }
      },
      {
        "cond": {
          "gte": "100",
          "lt": "1000"
        },
        "params": {
          "round": 2
        }
      },
      {
        "cond": {
          "gte": "0.001",
          "lt": "1",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "round": 3,
          "sign": true
        }
      },
      {
        "cond": {
          "gte": "0.001",
          "lt": "1"
        },
        "params": {
          "round": 3
        }
      },
      {
        "cond": {
          "gte": "0",
          "lt": "0.001",
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "round": 4,
          "sign": true
        }
      },
      {
        "cond": {
          "gte": "0",
          "lt": "0.001"
        },
        "params": {
          "round": 4
        }
      },
      {
        "cond": {
          "types": [
            "abs_diff"
          ]
        },
        "params": {
          "round": 2,
          "sign": true
        }
      }
    ]
  }
}
