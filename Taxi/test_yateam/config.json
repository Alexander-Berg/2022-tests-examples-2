{
  "FEMIDA_AUTHPROXY_ROUTE_RULES2": [
    {
      "input": {
        "http-path-prefix": "/_api/hire_orders/"
      },
      "output": {
        "attempts": 2,
        "timeout-ms": 5000,
        "tvm-service": "femida",
        "upstream": "http://femida"
      },
      "proxy": {
        "auth": "token",
        "passport-scopes": [
          "femida:all"
        ],
        "proxy-401": false,
        "server-hosts": [
          "*"
        ]
      }
    }
  ],
  "PASSPORT_BB_URL": {
    "$mockserver": "/blackbox"
  },
  "PASSPORT_BB_YANDEX_TEAM_URL": {
    "$mockserver": "/blackbox-team"
  }
}
