{
  "EATS_RESTAPP_AUTHPROXY_PREFIX_ACCESS_RULES": [
    {
      "access_rules": [
        {
          "access_conditions": {
            "any_of": [
              {
                "all_of": [
                  {
                    "name": "orders_common",
                    "type": "permission"
                  },
                  {
                    "name": "orders_cancel",
                    "type": "permission"
                  }
                ]
              }
            ]
          },
          "check_restrictions": false,
          "method": "POST",
          "path": "/4.0/restapp-front/api/v1/client/orders/cancel"
        },
        {
          "access_conditions": {
            "any_of": [
              {
                "all_of": [
                  {
                    "name": "orders_common",
                    "type": "permission"
                  }
                ]
              }
            ]
          },
          "check_restrictions": false,
          "method": "POST",
          "path": "/4.0/restapp-front/api/v1/client/orders/search"
        },
        {
          "access_conditions": {
            "any_of": [
              {
                "all_of": [
                  {
                    "name": "orders_changes",
                    "type": "permission"
                  }
                ]
              }
            ]
          },
          "check_restrictions": false,
          "method": "POST",
          "path": "/4.0/restapp-front/api/v1/client/orders/change"
        }
      ],
      "prefix": "/4.0/restapp-front/api/v1/client/orders"
    }
  ],
  "EATS_RESTAPP_AUTHPROXY_ROUTE_RULES": [
    {
      "input": {
