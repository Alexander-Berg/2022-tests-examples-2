      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {}
    },
    {
      "input": {
        "http-path-prefix": "/4.0/restapp-front/api/v1/client/restaurants"
      },
      "output": {
        "tvm-service": "mock",
        "upstream": {
          "$mockserver": ""
        }
      },
      "proxy": {}
    }
  ]
}
