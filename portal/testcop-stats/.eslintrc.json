{
    "env": {
        "es2021": true
    },
    "extends": [
        "yandex"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "rules": {
        "new-cap": "off",
        "only-var/only-var": "off",
        "strict": "off"
    },
    "ignorePatterns": ["build", "node_modules"]
}
