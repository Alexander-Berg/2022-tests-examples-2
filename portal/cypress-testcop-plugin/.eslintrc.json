{
    "env": {
        "es2021": true
    },
    "extends": [
        "yandex",
        "plugin:cypress/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "cypress",
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "globals": {
        "cypress/globals": true
    },
    "rules": {
        "new-cap": "off",
        "only-var/only-var": "off",
        "strict": "off"
    },
    "ignorePatterns": ["build", "node_modules"]
}
