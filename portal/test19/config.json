    "sourceMask": "pages/*.view-list.json",
    "destDir": "result",
    "destView": "desktop.view.js",
    "langFile": "lang.json",
    "command": "command",
    "includePath": ".",
    "langList": ["ru", "uk"],
    "output": {
        "requiredLang": true,
        "watch": true
    },
    "levels": [{
        "path": "blocks",
        "view": "home.desktopViews['<folder>']",
        "target": "some.view.js"
    }]
}
