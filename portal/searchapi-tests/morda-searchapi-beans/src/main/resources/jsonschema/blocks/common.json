{
  "non-empty-string": {
    "type": "string",
    "minLength": 0
  },
  "color": {
    "type": "string",
    "format": "color"
  },
  "temperature": {
    "type": "string",
    "pattern": "^0|([\\+\\-][1-9]\\d{0,2}?)$"
  },
  "time-hhmm": {
    "type": "string",
    "pattern": "^\\d{1,2}\\:\\d{2}$"
  }
}
