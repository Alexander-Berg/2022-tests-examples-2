@import '{{RELATIVEROOT}}/blocks/fonts/fonts.styl'

html
  box-sizing: border-box
  height: 100%

body
  width: 100%
  height: 100%
  box-sizing: border-box
  margin: 0
  padding: 0
  -webkit-overflow-scrolling: touch
  background-color: var(--color-bg-push-default)
  font-family: 'YS Text', Arial, sans-serif
  overflow-x: hidden
  -webkit-tap-highlight-color: transparent
