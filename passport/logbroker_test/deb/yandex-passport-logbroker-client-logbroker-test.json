{
    "meta" : {
        "name" : "yandex-passport-logbroker-client-logbroker-test",
        "maintainer" : "Passport team <passport-dev@yandex-team.ru>",
        "description" : "Passport Logbroker Client Logbroker Test",
        "version" : "{changelog_version}",
        "depends": [
            "init-system-helpers",
            "yandex-yasmagent",
            "passport-admin-config-golovan (>= 0.6)",
            "yandex-passport-xunistater (>= 2.7.0)"
        ]
    },
    "build" : {
        "targets" :  [
            "passport/backend/logbroker_client/logbroker_test/bin"
        ],
        "build_type": "release"
    },
    "data" : [
        {
            "source" : {
                "type" : "BUILD_OUTPUT",
                "path" : "passport/backend/logbroker_client/logbroker_test/bin",
                "files": [
                    "logbroker-client-logbroker-test"
                ]
            },
            "destination" : {
                "path" : "/usr/bin/"
            }
        },
        {
            "source" : {
                "type" : "ARCADIA",
                "path" : "passport/backend/logbroker_client/logbroker_test/configs",
                "files": ["*"]
            },
            "destination" : {
                "path" : "/etc/yandex/logbroker-client/logbroker-test/"
            }
        },
        {
            "source": {
                "type": "DIRECTORY"
            },
            "destination": {
                "path": "/var/log/yandex/logbroker-client/logbroker-test"
            }
        },
        {
            "source" : {
                "type" : "ARCADIA",
                "path" : "passport/backend/logbroker_client/logbroker_test/deb/debian",
                "files" : [
                    "yandex-passport-logbroker-client-logbroker-test.service"
                ]
            },
            "destination" : {
                "path" : "/lib/systemd/system/"
            }
        },
        {
            "source" : {
                "type" : "ARCADIA",
                "path" : "passport/backend/logbroker_client/logbroker_test/deb/xunistater/etc/yandex/passport-xunistater",
                "files" : [
                    "*.conf"
                ]
            },
            "destination" : {
                "path" : "/etc/yandex/passport-xunistater/"
            }
        },
        {
            "source" : {
                "type" : "ARCADIA",
                "path" : "passport/backend/logbroker_client/logbroker_test/deb/xunistater/usr/local/yasmagent/CONF",
                "files" : [
                    "*.conf"
                ]
            },
            "destination" : {
                "path" : "/usr/local/yasmagent/CONF/"
            }
        }
    ]
}
