---
topic: historydb2/testing/push
logger:
  level: 6
  file: /var/log/statbox/passport-push.log
  telemetry_interval: -1
  remote: 0
watcher:
  state: /var/lib/passport-push
  drop_on_error: 1
network:
  proto: pq
  master-addr: logbroker.yandex.net
  tvm-server-id: 2001059
  tvm-client-id: 2000904
  tvm-secret-file: /etc/yandex/statbox-push-client/push-client-logbroker-client.tvm
files:
  - name: /var/log/yandex/logbroker-client/challenge-pushes/statbox/passport_push.log
