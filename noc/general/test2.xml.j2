<include>
{% for item in regrtcom if inventory_hostname in item.host %}
{#{% for (name, secret) in regsecrets.items()%}}#}
        <gateway name="{{ item.name }}">
           <param name="username" value="{{ item.user }}"/>
           <param name="from-user" value="{{ item.user }}"/>
          <param name="password" value="{{ regsecrets.value[item.user] }}"/>
         <!--  <param name="from-domain" value=""/> -->
           <param name="realm" value="{{ item.realm }}"/>
           <param name="proxy" value="{{ item.proxy }}:{{ item.port }}"/>
           <param name="expire-seconds" value="60"/>
           <param name="register" value="true"/>
           <param name="retry-seconds" value="30"/>
        </gateway>
{% endfor %}
</include>