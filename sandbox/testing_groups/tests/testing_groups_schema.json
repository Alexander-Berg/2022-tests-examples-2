{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": {
        "type": "object",
        "required": [
            "components",
            "participants",
            "telegram",
            "asessors_priority",
            "activity",
            "manual_ticket_queue",
            "assessors_ticket_queue"
        ],
        "properties": {
            "components": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "participants": {
                "type": "array",
                "minItems": 1,
                "items": {
                    "type": "string"
                }
            },
            "telegram": {
                "type": "string",
                "minLength": 1
            },
            "manual_ticket_queue": {
                "type": "string",
                "minLength": 1
            },
            "assessors_ticket_queue": {
                "type": "string",
                "minLength": 1
            },
            "asessors_priority": {
                "type": "integer"
            },
            "testpalm_projects": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "activity": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "manual_ticket_tags": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "manual_ticket_components": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "asessors_ticket_tags": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "asessors_ticket_components": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "hardcore_assignee": {
                "type": "boolean"
            }
        }
    }
}
