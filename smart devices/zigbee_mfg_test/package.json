{
    "meta": {
        "name": "smart_devices_zigbee_mfg_test",
        "maintainer": "YandexIO team <quasar-team@yandex-team.ru>",
        "description": "Zigbee manufacturing test utility",
        "version": "{revision}.{branch}"
    },
    "build": {
        "armv7": {
            "targets": [
                "smart_devices/tools/zigbee_mfg_test"
            ],
            "flags": [
                {
                    "name": "STRIP",
                    "value": "yes"
                }
            ],
            "target-platforms": [
                "default-linux-armv7ahf_cortex_a53"
            ]
        },
        "x86_64": {
            "targets": [
                "smart_devices/tools/zigbee_mfg_test"
            ],
            "flags": [
                {
                    "name": "STRIP",
                    "value": "yes"
                }
            ],
            "target-platforms": [
                "default-linux-x86_64"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "armv7",
                "path": "smart_devices/tools/zigbee_mfg_test/zigbee_mfg_test"
            },
            "destination": {
                "path": "/armv7/zigbee_mfg_test"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "x86_64",
                "path": "smart_devices/tools/zigbee_mfg_test/zigbee_mfg_test"
            },
            "destination": {
                "path": "/x86_64/zigbee_mfg_test"
            }
        }
    ]
}
