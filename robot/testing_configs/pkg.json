{
    "meta": {
        "name": "smelter_testing_configs",
        "maintainer": "Nikolay Polivanov <niknik@yandex-team.ru>",
        "description": "SMELTER configs",
        "version": "no_version"
    },
    "build": {
        "targets": [
            "robot/smelter/packages/testing_configs"
        ],
        "flags": [
            { "name": "DEBUGINFO_LINES_ONLY", "value": "yes" }
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "robot/smelter/packages/testing_configs",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
