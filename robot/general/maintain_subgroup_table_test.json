{
    "HttpServer": {
        "Port": 25867
    },
    "Logs": {
        "Rules": [
            {
                "FilePath": "/dev/stderr",
                "MinLevel": "Info"
            },
            {
                "FilePath": "./chandra.log",
                "MinLevel": "Warning"
            }
        ]
    },

    "GlobalLogVerbosity": 7,

    "YtCluster": "arnold.yt.yandex.net",

    "YtMirrorSubgroupTablePath": "//home/robot-dev/agerria/UrlSubgroupTable",

    "ShardProcessorConfig": {
        "CommitPeriodMs": 3000,
        "DelayOnCommitErrorMs": 10000
    },

    "Suppliers": [
        {
            "Alias": "chandra_queue",

            "YtSupplier": {
                "Cluster": "arnold.yt.yandex.net",
                "QueuePath": "//home/robot-dev/agerria/MirrorsMainQueue",
                "QueueConsumer": "ray",
                "ChunkSize": 1,
                "FlushChunkIntervalMs": 5000,
                "MaxOutChunkSize": 1
            }
        }
    ],

    "Consumer": {
        "MainPath": "//home/robot-dev/agerria/MirrorsMainQueue/consuming_system",
        "MaxShards": 10,
        "MaxShardsToCheckAutoResolveRatio": 0.05,
        "MaxMessagesInProxyQueue": 5,
        "DelayOnUnequalShardOffsetsMs": 10000
    },


    "MaxInflightBytes": 1000000
}
