{
    "HttpServer": {
        "Port": 8888
    },
    "Logs": {
        "Rules": [
            {
                "FilePath": "/dev/stderr",
                "MinLevel": "Info"
            },
            {
                "FilePath": "./resharder.log",
                "MinLevel": "Warning"
            }
        ]
    },

    "GlobalLogVerbosity": 5,

    "YtCluster": "arnold.yt.yandex.net",
    
    "YtOutputQueuePath": "//home/robot-dev/agerria/BigRTCli",

    "ShardProcessorConfig": {
        "CommitPeriodMs": 3000,
        "DelayOnCommitErrorMs": 10000
    },

    "Suppliers": [
        {
            "Alias": "resharder_queue",

            "YtSupplier": {
                "Cluster": "arnold.yt.yandex.net",
                "QueuePath": "//home/robot-dev/agerria/JupiterItems",
                "QueueConsumer": "rsconsumer",
                "ChunkSize": 1000,
                "FlushChunkIntervalMs": 5000,
                "MaxOutChunkSize": 500
            }
        }
    ],

    "Consumer": {
        "MainPath": "//home/robot-dev/agerria/JupiterItems/consuming_system",
        "MaxShards": 10,
        "MaxShardsToCheckAutoResolveRatio": 0.05,
        "MaxMessagesInProxyQueue": 5,
        "DelayOnUnequalShardOffsetsMs": 10000
    },


    "MaxInflightBytes": 1000000
}
