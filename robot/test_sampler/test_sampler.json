{
    "TvmConfig": {
        "CacheDir": "./tvm_cache/",
        "DstServices": [
            {
                "Alias": "logbroker",
                "Id": 2001059
            }
        ],
        "SrcIdEnvName": "TVM_ID",
        "SrcSecretEnvName": "TVM_TOKEN"
    },

    "Logs": {
        "Rules": [
            {
                "FilePath": "/dev/stderr",
                "MinLevel": "Info"
            },
            {
                "FilePath": "./test_sampler.log",
                "MinLevel": "Warning"
            }
        ]
    },

    "GlobalLogVerbosity": 5,

    "YtCluster": "arnold.yt.yandex.net",
    "YtPath": "//home/robot-dev/agerria/TVMData",

    "ShardProcessorConfig": {
        "CommitPeriodMs": 3000,
        "DelayOnCommitErrorMs": 10000
    },

    "Suppliers": [
        {
            "Alias": "vla-logbroker",

            "LbSupplier": {
                "ConfigManagerGrpcHost": "cm.logbroker.yandex.net:1111",
                "LogbrokerClientId": "/callisto/callisto-mercury-staging",
                "LogbrokerTvmAlias": "logbroker",
                "SrcServer": "vla.logbroker.yandex.net",
                "SrcTopic": "/rthub/callisto"
            }
        },
        {
            "Alias": "sas-logbroker",

            "LbSupplier": {
                "ConfigManagerGrpcHost": "cm.logbroker.yandex.net:1111",
                "LogbrokerClientId": "/callisto/callisto-mercury-staging",
                "LogbrokerTvmAlias": "logbroker",
                "SrcServer": "sas.logbroker.yandex.net",
                "SrcTopic": "/rthub/callisto"
            }
        },
        {
            "Alias": "iva-logbroker",

            "LbSupplier": {
                "ConfigManagerGrpcHost": "cm.logbroker.yandex.net:1111",
                "LogbrokerClientId": "/callisto/callisto-mercury-staging",
                "LogbrokerTvmAlias": "logbroker",
                "SrcServer": "iva.logbroker.yandex.net",
                "SrcTopic": "/rthub/callisto"
            }
        }
    ],

    "Consumer": {
        "MainPath": "//home/robot-dev/agerria/TVMData/consuming_system",
        "MaxShards": 30,
        "MaxShardsToCheckAutoResolveRatio": 0.05,
        "MaxMessagesInProxyQueue": 100,
        "DelayOnUnequalShardOffsetsMs": 10000,
        "Shards": {
            "AutoResolve": {}
        },
        "Cluster": "arnold"
    },

    "MaxInflightBytes": 1000000
}
