    "tree-kill": "^1.2.0",
    "wf-bl": "6.0.11"
  },
  "scripts": {
    "all": "npm i && npm run tanker && npm run build",
    "precommit": "lint-staged",
    "start-intranet": "node worker.js intranet",
    "start-school": "node worker.js school",
    "start-business": "node worker.js business",
    "lint": "eslint .",
    "libs": "bower install --config.interactive=false --allow-root",
    "build": "enb make",
    "tanker": "node_modules/tanker-kit/bin/tanker pull",
    "stand": "releaser stand -s ${USER}${NUM} -i wiki-stand-front -a wiki-stand-front-intranet,wiki-stand-front-school,wiki-stand-front-business -c frontend",
    "release-patch": "npm version patch --no-git-tag-version",
    "release-minor": "npm version minor --no-git-tag-version",
    "release-major": "npm version major --no-git-tag-version",
    "postversion": "git add package.json package-lock.json && releaser release -v ${npm_package_version}",
    "unit-test": "NODE_ENV=mock jest",
    "hermione": "hermione",
    "palmsync-validate": "palmsync validate"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.deps.js": [
      "deps-formatter",
      "git add"
    ],
    "*.{css,styl}": [
      "stylelint"
    ]
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "roots": [
      "test"
    ]
  },
  "author": "Dmitry Tetchenko <tet4enko@yandex-team.ru>",
  "readmeFilename": "README.md",
  "private": true
}
