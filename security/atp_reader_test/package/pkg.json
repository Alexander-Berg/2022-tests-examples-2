{
    "meta": {
        "name": "atp-reader-test",
        "maintainer": "secsoc",
        "description": "test package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "security/soc/atp_reader_test"
        ]
    },
        "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "../src",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/opt/atp/src/"
            }
        }
    ]
}
